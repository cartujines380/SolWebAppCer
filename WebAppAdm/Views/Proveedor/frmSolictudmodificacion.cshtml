@{
    Layout = "~/Views/Shared/_LayoutAdm.cshtml";
    ViewBag.Title = "frmSolictudmodificacion";
}
@section styles{
    <link href="~/assets/css/angular-busy.css" rel="stylesheet" />
}
<div class="breadcrumbs" id="breadcrumbs" style="top:0px;">
    <script type="text/javascript">
        try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
    </script>

    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="~/Home/Index">Inicio</a>
        </li>
        <li>
            <i class="ace-icon fa fa-user"></i>
            <a href="~/Home/Index">Proveedores</a>
        </li>

        <li>
            @*<i class="ace-icon fafa-book home-icon"></i>*@
            <i class="ace-icon glyphicon glyphicon-list-alt"></i>
            <a href="frmSolictud">Solicitud de Modificación</a>
        </li>
    </ul>
</div>

<div id="main-container" data-ng-controller="SolicitudController" style=" font-size:12px;">
    <div cg-busy="{promise:myPromise,message:message,backdrop:true}">
        <form name="indexform">
            @*Session Botones*@
            <div class="widget-box">
                <div class="widget" ng-hide="hideproveedor">
                    <h4 class="smaller" style="text-align:right;">
                        <button type="submit" data-toggle="tooltip" title="{{Grabar}}" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="SaveProveedorList('0',indexform.$valid)">
                            <i data-toggle="tooltip" title="{{Grabar}}" class="menu-icon fa fa-save "></i>
                            {{Grabar}}
                        </button>

                        <button type="submit" class="btn btn-sm btn-round" title="{{Cancelar}}" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion1" ng-click="confirmaenviar()" data-ng-disabled='!indexform.$valid'>
                            <i data-toggle="tooltip" title="{{Cancelar}}" class="glyphicon glyphicon-cog "></i>
                            {{Cancelar}}
                        </button>
                        <br />
                    </h4>
                </div>
            </div>


            <div class="widget-box" ng-hide="hidecompras">
                <div class="widget">
                    <h4 class="smaller" style="text-align:right;">

                        <button data-toggle="tooltip" title="Revisado por Compras" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('RA',indexform.$valid)  ">
                            <i data-toggle="tooltip" title="Revisado por Compras" class="menu-icon glyphicon glyphicon-floppy-disk "></i>
                            Grabar
                        </button>

                        <button type="submit" data-toggle="tooltip" title="Revisado por Compras" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('RV',indexform.$valid)  ">
                            <i data-toggle="tooltip" title="Revisado por Compras" class="menu-icon glyphicon glyphicon-ok "></i>
                            Revisado por Compras
                        </button>

                        <button type="submit" data-toggle="tooltip" title="Corrección de Solicitud - Proveedor" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('DP',indexform.$valid)">
                            <i data-toggle="tooltip" title="Corrección de Solicitud - Proveedor" class="menu-icon glyphicon glyphicon-arrow-left "></i>
                            Corrección de Solicitud - Proveedor
                        </button>

                        <button type="submit" data-toggle="tooltip" title="Rechazado Asistentes" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('RE',indexform.$valid)">
                            <i data-toggle="tooltip" title="Rechazado Asistentes" class="menu-icon glyphicon glyphicon-remove "></i>
                            Rechazado Asistentes
                        </button>

                        <br />
                    </h4>
                </div>
            </div>

            <div class="widget-box" ng-hide="hidegerentecompras">
                <div class="widget">
                    <h4 class="smaller" style="text-align:right;">

                        <button type="submit" data-toggle="tooltip" title="Aprobado por Compras" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('AC',indexform.$valid)">
                            <i data-toggle="tooltip" title="Aprobado por Compras" class="menu-icon glyphicon glyphicon-ok "></i>
                            Aprobado por Compras
                        </button>

                        <button type="submit" data-toggle="tooltip" title="Rechazado Gerente" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('RC',indexform.$valid)">
                            <i data-toggle="tooltip" title="Rechazado Gerente" class="menu-icon glyphicon glyphicon-remove "></i>
                            Rechazado Gerente
                        </button>

                        <br />
                    </h4>
                </div>
            </div>

            <div class="widget-box" ng-hide="hidedatosmaestro">
                <div class="widget">
                    <h4 class="smaller" style="text-align:right;">
                        <button data-toggle="tooltip" title="Revisado por Compras" type="submit" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('RD',indexform.$valid)">
                            <i data-toggle="tooltip" title="Revisado por Compras" class="menu-icon glyphicon glyphicon-floppy-disk "></i>
                            Grabar
                        </button>

                        <button type="submit" data-toggle="tooltip" title="Devolver X  DM" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('DM',indexform.$valid)">
                            <i data-toggle="tooltip" title="Devolver X  DM" class="menu-icon glyphicon glyphicon-arrow-left "></i>
                            Devolver X  DM
                        </button>

                        <button type="submit" data-toggle="tooltip" title="Aprobar" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('AP',indexform.$valid)">
                            <i data-toggle="tooltip" title="Aprobar" class="menu-icon glyphicon glyphicon-ok "></i>
                            Aprobar
                        </button>

                        <br />
                    </h4>
                </div>
            </div>

            @*Fin Session Botones*@
            @*Inicio Session Tabs*@

            <div class="tabbable">
                <ul class="nav nav-tabs" id="myTab">
                    <li class="active">
                        <a data-toggle="tab" href="#datosproveedor">
                            Datos del Proveedor
                        </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#adjuntoproveedor">
                            Documentos Adjuntos
                        </a>
                    </li>
                    <li ng-hide="hidedatosmaestrocom">
                        <a data-toggle="tab" href="#datoscompras">
                            Compras - DM
                        </a>
                    </li>
                </ul>
                <div class="tab-content">
                    <!-- #section:Datos del Proveedor -->
                    <div id="datosproveedor" class="tab-pane fade in active">
                        <!--Inicio Identificacion -->
                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="widget-title" style="font-weight:normal">
                                    Identificación
                                </h4>
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="widget-body">
                                <div class="widget-main">

                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="form-group">

                                                <label class="col-sm-2 control-label no-padding-right" for="ddlTipoIndentificacion"> Tipo Identificación:</label>

                                                <div class="col-sm-2">

                                                    <select name="seletipoidentificacionProveedor" class="col-sm-2 chosen-select form-control" id="ddlTipoIndentificacion" data-ng-model="Ingreso.tipoidentificacion" data-placeholder="Seleccione un Tipo Identificación..." ng-options="item as item.detalle for item in ListTipoIdentificacion"
                                                            ng-required="true" oninvalid="setCustomValidity('Tipo de Identificacion es requerido')" oninput="setCustomValidity('')"
                                                            data-ng-class='{ error: indexform.seletipoidentificacionProveedor.$invalid && !indexform.$pristine }' disabled>
                                                        <option value=""> Seleccione un Tipo Identificación... </option>
                                                    </select>
                                                </div>

                                                <label class="col-sm-2 control-label" for="txtIdentificacion"> Identificación:*</label>

                                                <div class="col-md-6 ">
                                                    <input type="text" name="txtIdentificacion" placeholder="Identificación" class="col-xs-10 col-sm-5 no-padding form-control" ng-model="p_SolProveedor.Identificacion" maxlength="13" disabled onkeypress="return soloNumeros(event)"
                                                           ng-required="true" oninvalid="setCustomValidity('La identificacion es requerido')" oninput="setCustomValidity('')" minlength=" 10" data-ng-class='{ error: indexform.txtIdentificacion.$invalid && !txtIdentificacion.$pristine }' />
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12 ">
                                            <div class="form-group">
                                                <label class="col-md-2 control-label no-padding-right" for="txtNombreSri"> Nombre SRI:*</label>

                                                <div class="col-md-10">

                                                    <input type="text" name="txtNombreSri" ng-model="p_SolProveedor.RazonSocial" maxlength="140" placeholder="Nombre SRI" ng-disabled="isDisabledApr" class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;"
                                                           ng-required="true" oninvalid="setCustomValidity('Nombre SRI  es requerido')" oninput="    setCustomValidity('')" minlength="4" data-ng-class='{ error: indexform.txtNombreSri.$invalid && !txtNombreSri.$pristine }' />
                                                </div>


                                            </div>
                                        </div>
                                        <div class="col-md-12 ">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label no-padding-right" for="txtNombreComercial"> Nombre Comercial:*</label>
                                                <div class="col-md-10">
                                                    <input type="text"
                                                           name="txtNombreComercial"
                                                           placeholder="Nombre Comercial"
                                                           ng-model="p_SolProveedor.NomComercial"
                                                           class="form-control col-md-12 col-md-12 "
                                                           maxlength="20" ng-disabled="isDisabledApr" style=" text-transform:uppercase;"
                                                           ng-required="true"
                                                           oninvalid="setCustomValidity('Nombre Comercial  es requerido')" oninput="setCustomValidity('')"
                                                           minlength="4"
                                                           data-ng-class='{ error: indexform.txtNombreComercial.$invalid && !txtNombreComercial.$pristine }' />

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--fin Identificacion -->
                        <!--Inicio Ubicacion -->
                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="widget-title" style="font-weight:normal">
                                    Ubicación
                                </h4>
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="widget-body">
                                <div class="widget-main">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label no-padding-right" for="ddlPais">País:*</label>

                                                <div class="col-md-2">

                                                    <select name="selectpaisprov" class="form-control"
                                                            id="ddlPais" ng-disabled="isDisabledApr"
                                                            ng-model="Ingreso.Pais"
                                                            data-placeholder="Seleccione un Pais..."
                                                            ng-options="item as item.detalle for item in ListPais"
                                                            title="Seleccione un País..."
                                                            data-ng-class='{ error: indexform.selectpaisprov.$invalid && !indexform.$pristine }'
                                                            ng-required="true">
                                                        <option value=''>Seleccione un Pais...</option>
                                                    </select>




                                                </div>
                                                <div class="col-md-2">
                                                    <label class="col-md-2 control-label no-padding-right" for="ddlProvincia">Provincia:*</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <select name="selectprovinprov"
                                                            class="col-sm-2 chosen-select form-control"
                                                            id="ddlProvincia" data-placeholder="Seleccione una Provincia..."
                                                            data-ng-model=" Ingreso.Region" ng-disabled="isDisabledApr"
                                                            ng-options="item as item.detalle for item in ListRegionTemp | filter:{descAlterno: Ingreso.Pais.codigo}"
                                                            ng-required="true" required oninvalid="setCustomValidity('La Provincia es requerido')" oninput="    setCustomValidity('')"
                                                            data-ng-class='{ error: indexform.selectprovinprov.$invalid && !indexform.$pristine }'>
                                                        <option value=""> Seleccione una Provincia... </option>

                                                    </select>

                                                </div>
                                                <label class="col-sm-2 control-label no-padding-right" for="ddlCiudad">Ciudad:</label>

                                                <div class="col-sm-2">

                                                    <select class="col-sm-2 chosen-select form-control"
                                                            name="selectciudadnprov"
                                                            id="ddlCiudad"
                                                            data-placeholder="Seleccione un Ciudad..."
                                                            data-ng-model=" Ingreso.Ciudad"
                                                            ng-disabled="isDisabledApr"
                                                            ng-options="item as item.detalle for item in ListCiudadTemp | filter:{descAlterno: Ingreso.Region.codigo}">

                                                        <option value="">Seleccione una Ciudad...  </option>

                                                    </select>

                                                </div>


                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label no-padding-right" for="txtCallePrincipalNumero"> Calle Principal - Número:*</label>

                                                <div class="col-md-10">
                                                    <input type="text" name="txtCallePrincipalNumero" id="txtCallePrincipalNumero" ng-disabled="isDisabledApr" placeholder="Calle Principal - Número" ng-model="p_SolProvDireccion.CallePrincipal" class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;"
                                                           maxlength="30"
                                                           required
                                                           oninvalid="setCustomValidity('La Calle Principal  es requerido')"
                                                           oninput="setCustomValidity('')"
                                                           minlength="4"
                                                           data-ng-class='{ error: indexform.txtCallePrincipalNumero.$invalid && !txtCallePrincipalNumero.$pristine }' />
                                                </div>


                                            </div>
                                        </div>

                                        <div class="col-xs-12">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label no-padding-right" for="txtCalleSecundaria"> Calle Secundaria:</label>

                                                <div class="col-md-10">
                                                    <input type="text"
                                                           name="txtCalleSecundaria" id="txtCalleSecundaria"
                                                           ng-disabled="isDisabledApr"
                                                           placeholder="Calle Secundaria" style=" text-transform:uppercase;"
                                                           ng-model="p_SolProvDireccion.CalleSecundaria"
                                                           class="form-control col-md-12 col-md-12 " maxlength="40" />

                                                </div>


                                            </div>
                                        </div>

                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="col-md-2">
                                                    <label for="txtSolar">Solar:</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <input type="text" id="txtSolar" placeholder="Solar" ng-disabled="isDisabledApr" ng-model="p_SolProvDireccion.Solar" maxlength="30" style=" text-transform:uppercase;" class="form-control col-md-12 col-md-12 " />

                                                </div>
                                                <div class="col-md-2">
                                                    <label for="txtPisoOficina">Piso/Oficina:</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <input type="text" id="txtPisoOficina" ng-disabled="isDisabledApr" placeholder="Piso/Oficina" ng-model="p_SolProvDireccion.PisoEdificio" maxlength="10" class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;" />

                                                </div>
                                                <div class="col-md-2">
                                                    <label for="txtCodigoPostal">Código Postal:</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <input type="text" id="txtCodigoPostal" ng-disabled="isDisabledApr || bloqueCodPostal" placeholder="Código Postal" ng-model="p_SolProvDireccion.CodPostal" maxlength="30" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;" />

                                                </div>

                                            </div>

                                        </div>


                                    </div>

                                </div>
                            </div>
                        </div>
                        <!--Fin Ubicacion -->
                        <!--Inicio Contacto de la Empresa -->
                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="widget-title" style="font-weight:normal">
                                    Contactos Empresa
                                    <!--<small>different directions and colors</small>-->
                                </h4>
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>

                            <div class="widget-body">
                                <div class="widget-main">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label class="col-md-2 control-label no-padding-right" for="txtTelfijo">Teléfono Fijo:*</label>

                                                <div class="col-md-2">
                                                    <input type="text" name="txtTelfijo" id="txtTelfijo" ng-disabled="isDisabledApr" placeholder="Teléfono Fijo" ng-model="p_SolProveedor.TelfFijo" class="form-control col-md-12 col-md-12" maxlength="18" onkeypress="return soloNumeros(event)" ng-pattern="/^\d+$/"
                                                           required oninvalid="setCustomValidity('El Teléfono Fijo es requerido')" oninput="    setCustomValidity('')"
                                                           minlength="9" data-ng-class='{ error: indexform.txtTelfijo.$invalid && !txtTelfijo.$pristine }' />



                                                </div>
                                                <div class="col-md-2">

                                                </div>
                                                <div class="col-md-2">
                                                    <label for="txtTelFijoExt"> Ext.:</label>
                                                </div>

                                                <div class="col-md-1">
                                                    <input type="text" id="txtTelFijoExt" ng-disabled="isDisabledApr" placeholder="Ext." ng-model="p_SolProveedor.TelfFijoEXT" class="form-control  col-md-8 col-md-8" maxlength="4" onkeypress="return soloNumeros(event)" />

                                                </div>
                                                <label ng-hide="true" class=" col-sm-2 control-label no-padding-right" for="txtFax">Fax:</label>

                                                <div ng-hide="true" class="col-sm-2">
                                                    <input type="text"
                                                           name="txtFax"
                                                           id="txtFax"
                                                           ng-disabled="isDisabledApr"
                                                           placeholder="Fax"
                                                           class="form-control col-md-12 col-md-12"
                                                           ng-model="p_SolProveedor.TelfFax"
                                                           maxlength="18"
                                                           onkeypress="return soloNumeros(event)" />


                                                </div>
                                                <div ng-hide="true" class="col-md-1">
                                                    <label class="col-md-1 control-label no-padding-right" for="txtFaxExt">Ext.:</label>
                                                </div>
                                                <div ng-hide="true" class="col-md-1">
                                                    <input type="text"
                                                           id="txtFaxExt"
                                                           ng-disabled="isDisabledApr"
                                                           placeholder="Ext."
                                                           class="form-control col-md-8 col-md-8 " style=" text-transform:uppercase;"
                                                           ng-model="p_SolProveedor.TelfFaxEXT" maxlength="4"
                                                           onkeypress="return soloNumeros(event)" />

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="col-md-2">
                                                    <label for="TxtCelular"> Teléfono Celular:*</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <input type="text"
                                                           name="TxtCelular"
                                                           id="TxtCelular"
                                                           ng-disabled="isDisabledApr"
                                                           placeholder="Teléfono Celular - 10 números"
                                                           class="form-control col-md-12 col-md-12"
                                                           ng-model="p_SolProveedor.TelfMovil"
                                                           maxlength="18"
                                                           onkeypress="return soloNumeros(event)"
                                                           ng-pattern="/^\d+$/"
                                                           required
                                                           oninput="setCustomValidity('')"
                                                           minlength="10"
                                                           data-ng-class='{ error: indexform.TxtCelular.$invalid && !TxtCelular.$pristine }' />
                                                </div>
                                                <div class="col-md-2">

                                                </div>
                                                <div class="col-md-2">
                                                    <label for="txtMailCorpo"> Correo Corporativo:</label>
                                                </div>
                                                <div class="col-md-4">
                                                    <input type="email" name="txtMailCorpo" id="txtMailCorpo" @*ng-disabled="isDisabledApr"*@ placeholder="Correo Corporativo." class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;" ng-model="p_SolProveedor.EMAILCorp" maxlength="40"
                                                           required oninvalid="setCustomValidity('El  Correo Corporativo.  es requerido')" oninput="    setCustomValidity('')"
                                                           minlength="4" data-ng-class='{ error: indexform.txtMailCorpo.$invalid && !txtMailCorpo.$pristine }' disabled />


                                                </div>


                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--fin Contacto de la Empresa -->
                        <!--Inicio Línea de negocio de la Empresa -->
                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="widget-title" style="font-weight:normal">
                                    Línea de Negocio
                                    <!--<small>different directions and colors</small>-->
                                </h4>
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>

                            <div class="widget-body" ng-show="true">
                                <div class="widget-main">

                                    <div class="span" ng-controller="PaginationController">
                                        <div style=" height: 200px !important;overflow: scroll;">
                                            <div id=" no-more-tables">
                                                <table class="table table-striped table-bordered table-condensed table-hover gridStyle">
                                                    <thead>
                                                        <tr>
                                                            <th class="grid-header uppercase info" st-sort="Código" style="text-align:center; font-size:11px;font-family:Arial">

                                                                <a href="#" ng-click="sortType = 'codigo'; sortReverse = !sortReverse">
                                                                    Código
                                                                    <span ng-show="sortType == 'codigo' && !sortReverse" class="fa fa-caret-down"></span>
                                                                    <span ng-show="sortType == 'codigo' && sortReverse" class="fa fa-caret-up"></span>
                                                                </a>
                                                            </th>
                                                            <th class="grid-header uppercase info" st-sort="Línea de negocio" style="text-align:center; font-size:11px;font-family:Arial">

                                                                <a href="#" ng-click="sortType = 'descripcion'; sortReverse = !sortReverse">
                                                                    Línea de negocio
                                                                    <span ng-show="sortType == 'descripcion' && !sortReverse" class="fa fa-caret-down"></span>
                                                                    <span ng-show="sortType == 'descripcion' && sortReverse" class="fa fa-caret-up"></span>
                                                                </a>
                                                            </th>

                                                            <th class="grid-header uppercase info" st-sort="Seleccionar" style="text-align:center; font-size:11px;font-family:Arial">
                                                                <a href="#">
                                                                    Seleccionar
                                                                </a>
