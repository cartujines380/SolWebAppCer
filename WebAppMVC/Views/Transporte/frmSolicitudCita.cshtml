                                                                            </td>
                                                                            <td style="font-size:11px;text-align:center" data-title="Id Consolidacion">{{content.idconsolidacion}}</td>
                                                                            <td style="font-size:11px;text-align:center" data-title="Id Solicitud Cita" ng-if="content.variacita === '0'"></td>
                                                                            <td style="font-size:11px;text-align:center" data-title="Id Solicitud Cita" ng-if="content.variacita !== '0'">{{content.variacita}}</td>
                                                                            <td style="font-size:11px;text-align:center" data-title="Emision">{{content.emision}}</td>
                                                                            <td style="font-size:11px;" data-title="Almacen Destino">{{content.almacendestino}}</td>
                                                                            @*<td style="font-size:11px;text-align:center" data-title="Estado Consolidacion">{{content.estadoconsolidacion}}</td>*@
                                                                            <td style="font-size:11px;" data-title="Caducidad Consolidación">{{content.caducidadsolicitud}}</td>
                                                                            <td style="font-size:11px;" data-title="Vehiculo">{{content.vehiculo}}</td>
                                                                            <td style="font-size:11px;" data-title="Chofer">{{content.chofer}}</td>
                                                                            <td style="font-size:11px;" data-title="Asistencia">{{content.asistente}}</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                        <ul style="position: relative;" class="pagination">
