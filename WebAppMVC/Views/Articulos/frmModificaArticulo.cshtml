    <form name="observaform" id="observaform" ng-submit="NewArticulo2(codSapArt);codSapArt='';">
        @*<div class=" main-container" id="main-container">*@
        <div class="modal fade" data-backdrop="static" data-keyboard="false" id="modal-form-observacion_Agregar" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%;">
            @*<div id="modal-form-observacion_Agregar" tabindex="-1" class="modal fade" data-backdrop="static" style="height: 100%; width: 50%; " data-keyboard="false" role="dialog" aria-hidden="true">*@
            <div class="modal-dialog" style="padding-top:8%; height:80%; width:30%;">
                <div class="modal-content">
                    <div class="modal-header ">
                        <div class="widget-box">
                            <div class="widget-header">
                                <button type="button" class="close" data-dismiss="modal" ng-click="limpiamotivo()">&times;</button>
                                <h4 class="bigger">
                                    Agregar Artículo
                                </h4>
                            </div>
                        </div>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-12 col-md-12">




                                <div class="form-group">
                                    <label class="col-md-3 control-label no-padding-right" for="form-field-1   ">Cod. SAP artículo:</label>
                                    <div class="col-md-9">

                                        <input name="observacioncomentario" class="form-control" type="text" id="form-field-Segundo Nombre" placeholder="Código SAP artículo" value=""
                                               ng-required="true" oninvalid="setCustomValidity('Ingrese Cod. SAP de artículo')" oninput="    setCustomValidity('')"
                                               maxlength="18" data-ng-model="codSapArt"
                                                data-ng-class='{ error: observaform.observacioncomentario.$invalid && !observacioncomentario.$pristine }' />
                                    </div>
                                </div>
                                <div class="space-4"></div>
                                <div class="form-group">
                                    <div class="col-md-12">
                                    </div>
                                </div>



                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer ">
                    <button class="btn btn-sm" data-dismiss="modal" ng-click="codSapArt='';">
                        <i class="ace-icon fa fa-times"></i>
                        Cancelar
                    </button>
                    <button type="submit" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34) !important" id="btnConsulta" @*ng-click="SaveProveedorList('0', observaform.$valid) data-ng-disabled='!observaform.$valid'*@>
                        <i class="glyphicon glyphicon-cog "></i>
                        Aceptar
                    </button>
                </div>
            </div>

        </div>
        @*</div>*@

    </form>
    <!-- ******************** Fin Mensaje Confirmacion para Agregar Articulo *********************-->
    @Html.Partial("_mensajeError")
    @Html.Partial("_mensajeConfirmacion")
    @Html.Partial("_mensajeInformativo")
    @Html.Partial("_mensajeConfirmacionEliminar")
    @Html.Partial("_mensajeConfirmado")
    @Html.Partial("_mensajeOK")
