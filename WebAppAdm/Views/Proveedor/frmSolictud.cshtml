                                                <label class="col-md-3 control-label no-padding-right" for="form-field-1">No. ABA/IBAN:</label>
                                                <div class="col-md-3">
                                                    <input ng-disabled="isDisabledNomBanco" data-ng-model="p_SolProvBanco.CodABA" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" maxlength="10" type="text" id="form-field-1" placeholder="No. ABA/IBAN" class="col-md-12 col-md-12" />
                                                </div>
                                                <div class="col-md-2">
                                                    <label class="col-md-2 control-label no-padding-right" for="ddlProvincia">Región: </label>
                                                </div>
                                                <div class="col-md-4">
                                                    <select name="selectprovinbanco" ng-disabled="isDisabledApr" class="col-sm-2 chosen-select form-control" id="ddlProvinciabanco" data-placeholder="Seleccione una Provincia.." data-ng-model=" Ingreso.Regionbanco" ng-disabled="isDisabledApr" ng-options="item as item.detalle for item in ListRegionbancoTemp | filter:{descAlterno: Ingreso.Paisbanco.codigo}">
                                                        <option value=""> Seleccione una Región... </option>
                                                    </select>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="space-4"></div>
                                        <div class="row">
                                            <div class="form-group">
                                                <label class="col-md-3 control-label no-padding-right" for="form-field-1">Dirección Banco Extranjero:</label>
                                                <div class="col-md-9">
                                                    <input ng-disabled="isDisabledNomBanco" data-ng-model="p_SolProvBanco.DirBancoExtranjero" style=" text-transform:uppercase;" class="col-md-12 col-md-12 " type="text" id="form-field-Segundo Nombre" placeholder="Direccion de Banco Extranjero" maxlength="250" value="" />
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer ">
                            <button class="btn btn-sm" data-dismiss="modal" ng-click="limpiaadjunto()">
                                <i class="ace-icon glyphicon glyphicon-ban-circle"></i>
                                Cancelar
                            </button>
                            <button type="submit" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnConsulta" @*ng-click="adicionabanco()" data-ng-disabled='!bancoform.$valid'*@>
                                <i class="glyphicon glyphicon-plus"></i>
                                {{Grabarbanco}}
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </form>
        @*Session modal Observacion*@
        <form name="observaform" id="observaform" ng-submit="SaveProveedorList('0', observaform.$valid)">
            <div class=" main-container" id="main-container">
                <div id="modal-form-observacion" class="modal fade" data-backdrop="static" style="height: 100%; width: 100%; z-index: 90;" data-keyboard="false" role="dialog" aria-hidden="true" tabindex="-1">
                    <div class="modal-dialog" style="padding-top: 10%; height:80%; width:60%;">
                        <div class="modal-content">
                            <div class="modal-header ">
                                <div class="widget-box">
                                    <div class="widget-header">
                                        <button type="button" class="close" data-dismiss="modal" ng-click="limpiamotivo()">&times;</button>
                                        <h4 class="bigger">
                                            Motivos
                                        </h4>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-md-12 col-md-12">
                                        <div class="form-group">
                                            <label class="col-md-3 col-md-12 control-label no-padding-right" for="form-field-1"> Tipo Identificación: </label>
                                            <div class="col-md-3">
                                                <select ng-disabled="true" class="form-control" id="form-field-select-3" data-placeholder="Seleccione un Tipo Identificación.." data-ng-model="Ingreso.tipoidentificacion" ng-options="item as item.detalle for item in ListTipoIdentificacion"
                                                        ng-required="true" oninvalid="setCustomValidity('Tipo Identificación  es  requerido')" oninput="    setCustomValidity('')">
                                                    <option value="">Seleccione un Tipo de Identificación...</option>
                                                </select>
                                            </div>
                                            <label class="col-md-2 control-label no-padding-right" for="form-field-1"> Identificación: </label>
                                            <div class="col-md-4">
                                                <input ng-disabled="true" type="text" id="form-field-1" placeholder="Identificación" class="col-md-12 col-md-12"
                                                       ng-pattern="/^\d+$/" data-ng-model="p_SolProveedor.Identificacion" />

                                            </div>
                                        </div>
                                        <div class="space-4"></div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label no-padding-right" for="form-field-username">Nombre Comercial:</label>
                                            <div class="col-md-9">
                                                <input ng-disabled="true" name="" style=" text-transform:uppercase;" class="form-control col-md-12 col-md-12 " type="text" id="form-field-username" placeholder="Primer Nombre" value="" data-ng-model="p_SolProveedor.NomComercial" />
                                            </div>
                                        </div>
                                        <div class="space-4"></div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-3 control-label no-padding-right" for="form-field-1">Motivo:</label>

                                            <div class="col-md-9">


                                                <select name="seleconamotivo" class="form-control col-md-12" id="form-field-select-3" data-placeholder="Seleccione un Motivo.." data-ng-model="Ingreso.MotivoRechazoProveedor" ng-options="item as item.detalle for item in ListMotivoRechazoProveedor"
                                                        ng-required="true" oninvalid="setCustomValidity('El Motivo es  requerido')" oninput="    setCustomValidity('')"
                                                        required data-ng-class='{ error: observaform.seleconamotivo.$invalid && !observaform.$pristine }'>
                                                    <option value="">Seleccione un Motivo...</option>

                                                </select>



                                            </div>

                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-3 control-label no-padding-right" for="form-field-1	">Observación:</label>
                                            <div class="col-md-9">
                                                <input name="observacioncomentario" style=" text-transform:uppercase;" class="form-control col-md-12 col-md-12 " type="text" id="form-field-Segundo Nombre" placeholder="Observación" value=""
                                                       ng-required="true" oninvalid="setCustomValidity('La Observación es  requerido')" oninput="    setCustomValidity('')"
                                                       maxlength="500" data-ng-model="p_SolProvHistEstado.Observacion"
                                                       data-ng-class='{ error: observaform.observacioncomentario.$invalid && !observacioncomentario.$pristine }' />
                                            </div>
                                        </div>
                                        <div class="space-4"></div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                            </div>
                                        </div>



                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer ">
                            <button class="btn btn-sm" data-dismiss="modal" ng-click="limpiamotivo()">
                                <i class="ace-icon glyphicon glyphicon-ban-circle"></i>
                                Cancelar
                            </button>
                            <button class="btn btn-sm" data-dismiss="modal" ng-click="SaveProveedorList('0', true)" ng-hide="true">
                                <i class="ace-icon glyphicon glyphicon-check"></i>
                                Omitir
                            </button>
                            <button type="submit" class="btn btn-sm btn-round" style="background-color:#006865!important;border-color:#006865 !important" id="btnConsulta" @*ng-click="SaveProveedorList('0', observaform.$valid) data-ng-disabled='!observaform.$valid'*@>
                                <i class="glyphicon glyphicon-check "></i>
                                Aceptar
                            </button>
                        </div>
                    </div>

                </div>
            </div>

        </form>
    </div>
</body>



