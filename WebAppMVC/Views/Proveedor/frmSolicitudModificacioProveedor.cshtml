@{
    Layout = "~/Views/Shared/_LayoutPrivado.cshtml";
    ViewBag.Title = "frmSolictud";
}
@section styles{
    <link href="~/assets/css/angular-busy.css" rel="stylesheet" />
    <link href="~/Content/Validation.css" rel="stylesheet" />
    <link href="~/assets/css/fileinput.css" rel="stylesheet" />
}

<div class="breadcrumbs" id="breadcrumbs" style="top:0px;">
    <script type="text/javascript">
        try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
    </script>

    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="~/Notificacion/frmVisualizaNotificaciones">Inicio</a>
        </li>
        <li>
            <i class="ace-icon fa fa-user"></i>
            <a href="~/Home/IndexPrivado">Proveedores</a>
        </li>

        <li>
            @*<i class="ace-icon fafa-book home-icon"></i>*@
            <i class="ace-icon glyphicon glyphicon-edit"></i>
            <a href="frmSolicitudModificacioProveedor">Solicitud de Modificación</a>
        </li>
    </ul>
</div>
<body class="page_container row ng-cloak" data-ng-controller="ProveedorModificacionController">
    <div id="main-container" style=" font-size:12px;">
        <div cg-busy="{promise:myPromise,message:message,backdrop:true}">
            <form name="indexform">
                @*Session Botones*@
                <div class="widget-box">
                    <div class="widget" ng-hide="hideproveedor">
                        <h4 class="smaller" style="text-align:right;">
                            <button type="submit" data-toggle="tooltip" title="{{Grabar}}" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="indexform.$valid && SaveProveedorList('0',indexform.$valid)">
                                <i data-toggle="tooltip" title="{{Grabar}}" class="menu-icon glyphicon glyphicon-floppy-disk"></i>
                                {{Grabar}}
                            </button>

                            <button type="submit" class="btn btn-sm btn-round btn-primary" title="{{Cancelar}}" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion1" ng-click="indexform.$valid && confirmaenviar()" data-ng-disabled='!indexform.$valid'>
                                <i data-toggle="tooltip" title="{{Cancelar}}" class="glyphicon glyphicon-send "></i>
                                {{Cancelar}}
                            </button>
                            <br />
                        </h4>
                    </div>
                </div>


                <div class="widget-box" ng-hide="hidecompras">
                    <div class="widget">
                        <h4 class="smaller" style="text-align:right;">

                            <button type="submit" data-toggle="tooltip" title="Revisado por Compras" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('RV',indexform.$valid)  " @*data-ng-disabled='!indexform.$valid'*@>
                                <i data-toggle="tooltip" title="Revisado por Compras" class="menu-icon fa fa-save "></i>
                                Revisado por Compras
                            </button>

                            <button type="submit" data-toggle="tooltip" title="Corrección de Solicitud - Proveedor" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('DP',indexform.$valid)" @*data-ng-disabled='!indexform.$valid'*@>
                                <i data-toggle="tooltip" title="Corrección de Solicitud - Proveedor" class="menu-icon fa fa-save "></i>
                                Corrección de Solicitud - Proveedor
                            </button>

                            <button type="submit" data-toggle="tooltip" title="Rechazado Asistentes" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('RE',indexform.$valid)" @*data-ng-disabled='!indexform.$valid'*@>
                                <i data-toggle="tooltip" title="Rechazado Asistentes" class="menu-icon fa fa-save "></i>
                                Rechazado Asistentes
                            </button>

                            <button type="submit" data-toggle="tooltip" title="Imprimir" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="" data-ng-disabled='!indexform.$valid'>
                                <i data-toggle="tooltip" title="Imprimir" class="menu-icon fa fa-save "></i>
                                Imprimir
                            </button>


                            <br />
                        </h4>
                    </div>
                </div>

                <div class="widget-box" ng-hide="hidegerentecompras">
                    <div class="widget">
                        <h4 class="smaller" style="text-align:right;">

                            <button type="submit" data-toggle="tooltip" title="Aprobado por Compras" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('AC',indexform.$valid)" @*data-ng-disabled='!indexform.$valid'*@>
                                <i data-toggle="tooltip" title="Aprobado por Compras" class="menu-icon fa fa-save "></i>
                                Aprobado por Compras
                            </button>

                            <button type="submit" data-toggle="tooltip" title="Rechazado Gerente" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('RC',indexform.$valid)" @*data-ng-disabled='!indexform.$valid'*@>
                                <i data-toggle="tooltip" title="Rechazado Gerente" class="menu-icon fa fa-save "></i>
                                Rechazado Gerente
                            </button>

                            <br />
                        </h4>
                    </div>
                </div>

                <div class="widget-box" ng-hide="hidedatosmaestro">
                    <div class="widget">
                        <h4 class="smaller" style="text-align:right;">

                            <button type="submit" data-toggle="tooltip" title="Devolver X  DM" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('DM',indexform.$valid)" @*data-ng-disabled='!indexform.$valid'*@>
                                <i data-toggle="tooltip" title="Devolver X  DM" class="menu-icon fa fa-save "></i>
                                Devolver X  DM
                            </button>

                            <button type="submit" data-toggle="tooltip" title="Aprobar" type="button" class="btn btn-sm btn-round btn-primary" style="background-color:#006865!important;border-color:#006865 !important" id="btnaccion0" ng-click="enviarobservacion('AP',indexform.$valid)" @*data-ng-disabled='!indexform.$valid'*@>
                                <i data-toggle="tooltip" title="Aprobar" class="menu-icon fa fa-save "></i>
                                Aprobar
                            </button>

                            <br />
                        </h4>
                    </div>
                </div>

                @*Fin Session Botones*@
                @*Inicio Session Tabs*@

                <div class="tabbable">
                    <ul class="nav nav-tabs" id="myTab">
                        <li class="active">
                            <a data-toggle="tab" href="#datosproveedor">
                                Datos del Proveedor
                            </a>
                        </li>
                        @*<li>
                                <a data-toggle="tab" href="#contactoproveedor">
                                    Contacto
                                </a>
                            </li>*@
                        <li>
                            <a data-toggle="tab" href="#adjuntoproveedor">
                                Documentos Adjuntos
                            </a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <!-- #section:Datos del Proveedor -->
                        <div id="datosproveedor" class="tab-pane fade in active" @*ng-disabled="isdisGerente"*@>
                            <!--Inicio Identificacion -->
                            <div class="widget-box">
                                <div class="widget-header">
                                    <h4 class="widget-title" style="font-weight:normal">
                                        Identificación

                                    </h4>
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                    <div class="widget-toolbar">
                                        <a href="#" data-action="collapse">
                                            <i class="ace-icon fa fa-chevron-up"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="widget-body">
                                    <div class="widget-main">
                                        <div class="row">
                                            <div class="col-xs-12">
                                                <div class="form-group">

                                                    <label class="col-sm-2 control-label no-padding-right" for="ddlTipoIndentificacion"> Tipo Identificación: </label>

                                                    <div class="col-sm-2">
                                                        <!--<input type="text" id="form-field-1" placeholder="Username" class="col-xs-5 col-sm-5" />-->
                                                        <select name="seletipoidentificacionProveedor" class="col-sm-2 chosen-select form-control" id="ddlTipoIndentificacion" data-ng-model="Ingreso.tipoidentificacion" data-placeholder="Seleccione un Tipo Identificación.." ng-options="item as item.detalle for item in ListTipoIdentificacion"
                                                                ng-required="true" oninvalid="setCustomValidity('Tipo de Identificacion requerido')" oninput="    setCustomValidity('')"
                                                                data-ng-class='{ error: indexform.seletipoidentificacionProveedor.$invalid && !indexform.$pristine }' disabled>
                                                            <option value=""> Seleccione un Tipo Identificación... </option>

                                                        </select>

                                                    </div>

                                                    <label class="col-sm-2 control-label" for="txtIdentificacion"> Identificación: </label>

                                                    <div class="col-md-6 ">
                                                        <input type="text" name="txtIdentificacion" placeholder="Identificación" class="col-xs-10 col-sm-5 no-padding" ng-model="p_SolProveedor.Identificacion" maxlength="13" disabled onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;"
                                                               ng-required="true" oninvalid="setCustomValidity('La Identificacion requerido')" oninput="    setCustomValidity('')" minlength="10" data-ng-class='{ error: indexform.txtIdentificacion.$invalid && !txtIdentificacion.$pristine }' />


                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12 ">
                                                <div class="form-group">
                                                    <label class="col-md-2 control-label no-padding-right" for="txtNombreSri"> Nombre SRI:*</label>

                                                    <div class="col-md-10">

                                                        <input type="text" name="txtNombreSri" ng-model="p_SolProveedor.RazonSocial" maxlength="140" placeholder="Nombre SRI" ng-disabled="isDisabledApr" class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;"
                                                               ng-required="true" oninvalid="setCustomValidity('El Nombre SRI requerido')" oninput="    setCustomValidity('')"  data-ng-class='{ error: indexform.txtNombreSri.$invalid && !txtNombreSri.$pristine }' />

                                                    </div>


                                                </div>
                                            </div>
                                            <div class="col-md-12 ">
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label no-padding-right" for="txtNombreComercial"> Nombre Comercial:*</label>
                                                    <div class="col-md-10">
                                                        <input type="text" name="txtNombreComercial" placeholder="Nombre Comercial" style=" text-transform:uppercase;" ng-model="p_SolProveedor.NomComercial" class="form-control col-md-12 col-md-12" maxlength="20" ng-disabled="isDisabledApr"
                                                               ng-required="true" oninvalid="setCustomValidity('Nombre Comercial requerido')" oninput="    setCustomValidity('')" data-ng-class='{ error: indexform.txtNombreComercial.$invalid && !txtNombreComercial.$pristine }' />

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--fin Identificacion -->
                            <!--Inicio Ubicacion -->
                            <div class="widget-box">
                                <div class="widget-header">
                                    <h4 class="widget-title" style="font-weight:normal">
                                        Ubicación
                                    </h4>
                                    <div class="widget-toolbar">
                                        <a href="#" data-action="collapse">
                                            <i class="ace-icon fa fa-chevron-up"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="widget-body">
                                    <div class="widget-main">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label no-padding-right" for="ddlPais">País:*</label>
                                                    <div class="col-md-2">
                                                        <select class="col-md-2 chosen-select form-control" name="selectpaisprov" id="ddlPais" data-placeholder="Seleccione un Pais..." ng-init="Ingreso.Pais=ListPais[0]" data-ng-model=" Ingreso.Pais" ng-options="item as item.detalle for item in ListPais" ng-disabled="isDisabledApr"
                                                                ng-required="true" oninvalid="setCustomValidity('El País requerido')" oninput="    setCustomValidity('')"
                                                                required data-ng-class='{ error: indexform.selectpaisprov.$invalid && !indexform.$pristine }'>
                                                            <option value=""> Seleccione un País... </option>

                                                        </select>

                                                    </div>
                                                    <div class="col-md-2">
                                                        <label class="col-md-2 control-label no-padding-right" for="ddlProvincia">Provincia:*</label>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <select name="selectprovinprov"
                                                                class="col-sm-2 chosen-select form-control"
                                                                id="ddlProvincia" data-placeholder="Seleccione una Provincia..."
                                                                data-ng-model=" Ingreso.Region" ng-disabled="isDisabledApr"
                                                                ng-options="item as item.detalle for item in ListRegionTemp | filter:{descAlterno: Ingreso.Pais.codigo}"
                                                                ng-required="true" required oninvalid="setCustomValidity('La Provincia es requerido')" oninput="    setCustomValidity('')"
                                                                data-ng-class='{ error: indexform.selectprovinprov.$invalid && !indexform.$pristine }'>
                                                            <option value="">Seleccione una Provincia... </option>

                                                        </select>

                                                    </div>
                                                    <label class="col-sm-2 control-label no-padding-right" for="ddlCiudad">Ciudad: </label>
                                                    <div class="col-sm-2">
                                                        <select class="col-sm-2 chosen-select form-control"
                                                                name="selectciudadnprov" id="ddlCiudad"
                                                                data-placeholder="Seleccione un Ciudad..."
                                                                data-ng-model=" Ingreso.Ciudad" ng-disabled="isDisabledApr"
                                                                ng-options="item as item.detalle for item in ListCiudadTemp | filter:{descAlterno: Ingreso.Region.codigo}">
                                                            <option value=""> Seleccione una Ciudad... </option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label no-padding-right" for="txtCallePrincipalNumero"> Calle Principal - Número:*</label>
                                                    <div class="col-md-10">
                                                        <input type="text" name="txtCallePrincipalNumero" id="txtCallePrincipalNumero" ng-disabled="isDisabledApr" placeholder="Calle Principal - Número" ng-model="p_SolProvDireccion.CallePrincipal" style=" text-transform:uppercase;" class="form-control col-md-12 col-md-12 "
                                                               maxlength="30"
                                                               required oninvalid="setCustomValidity('La Calle Principal requerido')" oninput="    setCustomValidity('')"
                                                               data-ng-class='{ error: indexform.txtCallePrincipalNumero.$invalid && !txtCallePrincipalNumero.$pristine }' />

                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-xs-12">
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label no-padding-right" for="txtCalleSecundaria"> Calle Secundaria: </label>
                                                    <div class="col-md-10">
                                                        <input type="text" name="txtCalleSecundaria" id="txtCalleSecundaria" ng-disabled="isDisabledApr" placeholder="Calle Secundaria" ng-model="p_SolProvDireccion.CalleSecundaria" class="form-control col-md-12 col-md-12 " maxlength="40" style=" text-transform:uppercase;" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <div class="col-md-2">
                                                        <label for="txtSolar">Solar: </label>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <input type="text" id="txtSolar" placeholder="Solar" ng-disabled="isDisabledApr" ng-model="p_SolProvDireccion.Solar" maxlength="30" class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;" />
                                                    </div>
                                                    <div class="col-md-2">
                                                        <label for="txtPisoOficina">Piso/Oficina: </label>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <input type="text" id="txtPisoOficina" ng-disabled="isDisabledApr" placeholder="Piso/Oficina" ng-model="p_SolProvDireccion.PisoEdificio" maxlength="10" class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;" />
                                                    </div>
                                                    <div class="col-md-2">
                                                        <label for="txtCodigoPostal">Código Postal:</label>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <input type="text" id="txtCodigoPostal" ng-disabled="isDisabledApr || bloqueCodPostal" placeholder="Código Postal" ng-model="p_SolProvDireccion.CodPostal" maxlength="30" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" class="form-control col-md-12 col-md-12 " style=" text-transform:uppercase;" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--Fin Ubicacion -->
                            <!--Inicio Contacto de la Empresa -->
                            <div class="widget-box">
                                <div class="widget-header">
                                    <h4 class="widget-title" style="font-weight:normal">
                                        Contactos Empresa
                                        <!--<small>different directions and colors</small>-->
                                    </h4>
                                    <div class="widget-toolbar">
                                        <a href="#" data-action="collapse">
                                            <i class="ace-icon fa fa-chevron-up"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="widget-body">
                                    <div class="widget-main">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label class="col-md-2 control-label no-padding-right" for="txtTelfijo">Teléfono Fijo:*</label>
                                                    <div class="col-md-2">
                                                        <input type="text" name="txtTelfijo" id="txtTelfijo" ng-disabled="isDisabledApr" placeholder="Teléfono Fijo" ng-model="p_SolProveedor.TelfFijo" class="form-control col-md-12 col-md-12" maxlength="18" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" ng-pattern="/^\d+$/"
                                                               ng-required="true" oninvalid="setCustomValidity('El Telefono requerido')" oninput="    setCustomValidity('')"
                                                               minlength="9" data-ng-class='{ error: indexform.txtTelfijo.$invalid && !txtTelfijo.$pristine }' />


                                                    </div>
                                                    <div class="col-md-2">
                                                    </div>
                                                    <div class="col-md-2">
                                                        <label for="txtTelFijoExt"> Ext: </label>
                                                    </div>
                                                   
                                                    <div class="col-md-2">
                                                        <input type="text" id="txtTelFijoExt" ng-disabled="isDisabledApr" placeholder="Ext." ng-model="p_SolProveedor.TelfFijoEXT" class="form-control  col-md-8 col-md-8" maxlength="4" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" />
                                                    </div>
                                                    <label ng-hide="true" class=" col-sm-2 control-label no-padding-right" for="txtFax">Fax:</label>
                                                    <div ng-hide="true" class="col-sm-2">
                                                        <input type="text" name="txtFax" id="txtFax" ng-disabled="isDisabledApr" placeholder="Fax" class="form-control col-md-12 col-md-12" ng-model="p_SolProveedor.TelfFax" maxlength="18" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" />
                                                    </div>



                                                    <div ng-hide="true" class="col-md-1">
                                                        <label class="col-md-1 control-label no-padding-right" for="txtFaxExt">Ext: </label>
                                                    </div>
                                                    <div ng-hide="true" class="col-md-1">
                                                        <input type="text" id="txtFaxExt" ng-disabled="isDisabledApr" placeholder="Ext." class="form-control col-md-9 col-md-9" ng-model="p_SolProveedor.TelfFaxEXT" maxlength="4" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <div class="col-md-2">
                                                        <label for="TxtCelular"> Teléfono Celular:*</label>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <input type="text" name="TxtCelular" id="TxtCelular" ng-disabled="isDisabledApr" placeholder="Teléfono Celular" class="form-control col-md-12 col-md-12" ng-model="p_SolProveedor.TelfMovil" maxlength="18" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" ng-pattern="/^\d+$/"
                                                               ng-required="true" oninvalid="setCustomValidity('El Teléfono Celular  requerido')" oninput="    setCustomValidity('')"
                                                               minlength="10" pattern=".{10,}" data-ng-class='{ error: indexform.TxtCelular.$invalid && !TxtCelular.$pristine }' />
                                                    </div>
                                                    <div class="col-md-2">
                                                    </div>
                                                    <div class="col-md-2">
                                                        <label for="txtMailCorpo"> Correo Corporativo:* </label>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="email" name="txtMailCorpo" id="txtMailCorpo" style=" text-transform:uppercase;" ng-disabled="isDisabledApr" placeholder="Correo Corporativo" class="form-control col-md-12 col-md-12 " ng-model="p_SolProveedor.EMAILCorp" maxlength="40"
                                                               required oninvalid="setCustomValidity('El Correo Coporativo  requerido')" oninput="    setCustomValidity('')"
                                                               minlength="4" data-ng-class='{ error: indexform.txtMailCorpo.$invalid && !txtMailCorpo.$pristine }' />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--fin Contacto de la Empresa -->
                            <!--Inicio Línea de negocio de la Empresa -->
                            <div class="widget-box">
                                <div class="widget-header">
                                    <h4 class="widget-title" style="font-weight:normal">
                                        Línea de Negocio
                                        <!--<small>different directions and colors</small>-->
                                    </h4>
                                    <div class="widget-toolbar">
                                        <a href="#" data-action="collapse">
                                            <i class="ace-icon fa fa-chevron-up"></i>
                                        </a>
                                    </div>
                                </div>

                                <div class="widget-body" ng-show="true">
                                    <div class="widget-main">

                                        <div class="span" ng-controller="PaginationController">
                                            <div style=" height: 200px !important;overflow: scroll;">
                                                <div id=" no-more-tables">
                                                    <table class="table table-striped table-bordered table-condensed table-hover gridStyle">
                                                        <thead>
                                                            <tr>
                                                                <th class="grid-header uppercase info" st-sort="Código" style="text-align:center; font-size:11px;font-family:Arial">

                                                                    <a href="#" ng-click="sortType = 'codigo'; sortReverse = !sortReverse">
                                                                        Código
                                                                        <span ng-show="sortType == 'codigo' && !sortReverse" class="fa fa-caret-down"></span>
                                                                        <span ng-show="sortType == 'codigo' && sortReverse" class="fa fa-caret-up"></span>
                                                                    </a>
                                                                </th>
                                                                <th class="grid-header uppercase info" st-sort="Línea de negocio" style="text-align:center; font-size:11px;font-family:Arial">

