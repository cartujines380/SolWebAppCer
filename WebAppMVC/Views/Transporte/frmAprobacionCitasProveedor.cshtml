                                                                                <span ng-show="sortType == 'idcita' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info">
                                                                            <a href="#" ng-click="sortType = 'numcita'; sortReverse = !sortReverse">
                                                                                Núm. Cita
                                                                                <span ng-show="sortType == 'numcita' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'numcita' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info">
                                                                            <a href="#" ng-click="sortType = 'idconsolidacion'; sortReverse = !sortReverse">
                                                                                Id. Consolidación
                                                                                <span ng-show="sortType == 'idconsolidacion' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'idconsolidacion' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info">
                                                                            <a href="#" ng-click="sortType = 'bodega'; sortReverse = !sortReverse">
                                                                                Bodega
                                                                                <span ng-show="sortType == 'bodega' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'bodega' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info">
                                                                            <a href="#" ng-click="sortType = 'estado'; sortReverse = !sortReverse">
                                                                                Estado Cita
                                                                                <span ng-show="sortType == 'estado' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'estado' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info">
                                                                            <a href="#" ng-click="sortType = 'estadorechazo'; sortReverse = !sortReverse">
                                                                                Motivo
                                                                                <span ng-show="sortType == 'estadorechazo' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'estadorechazo' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info">
                                                                            <a href="#" ng-click="sortType = 'fechasolicitada'; sortReverse = !sortReverse">
