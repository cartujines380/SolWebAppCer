@{
    Layout = "~/Views/Shared/_LayoutAdm.cshtml";
    ViewBag.Title = "frmContacto";
}
@section styles{
<link href="~/Content/Validation.css" rel="stylesheet" />
    <link href="~/assets/css/angular-busy.css" rel="stylesheet" />
}
<div class="breadcrumbs" id="breadcrumbs" style="top:0px;">
    <script type="text/javascript">
        try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
    </script>

    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>&nbsp;<a href="~/Home/Index">Inicio</a>
        </li>
        <li>
            <i class="ace-icon fa fa-database"></i>&nbsp;<a href="~/Home/Index">Etiquetas</a>
        </li>
        <li>
            <i class="ace-icon fa fa-user"></i>&nbsp;<a href="frmAsignacionProv">Asignación Proveedor/Etiqueta</a>
        </li>
        @*<li class="active">Profile 1</li>*@
    </ul><!-- /.breadcrumb -->
</div>

<body class="page_container row ng-cloak" data-ng-controller="AsignacionProveedorController" >



    @*Session Busqueda*@
    <div style="height :100%; width:100%; font-size:12px;zoom:100%;">
        <div style="height:100%;">
            <div class="ace-settings-container" id="ace-settings-container">
                <div class="btn btn-app btn-xs btn-warning ace-settings-btn" id="ace-settings-btn">
                    <i class="glyphicon glyphicon-search"></i>
                </div>
                <div class="ace-settings-box clearfix" id="ace-settings-box">
                    
                        <div class="container container-fluid" style="padding:0 0 0 0">
                            <div class="row">
                                <div class="col-md-3" style="text-align: right;">

                                    @*<button type="button" class="btn btn-sm btn-round"
                                            style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" id="btnCons"
                                            ng-click="filtroConsulta3();">
                                        <i class="ace-icon fa fa-search"></i>
                                        Consultar
                                    </button>*@
                                    <button type="button" id="btnConsulta1" class="btn btn-sm btn-round"
                                            style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important;"
                                            create-pages results="ProveedorEti2" number-per-page="30" pages="pagesCon"
                                            page-content="ProveedorEti;" page-limit="9000" binding="blur"
                                            ng-click=" filtroConsulta3(); showpaginate=true;">
                                        <i class="glyphicon glyphicon-search"></i>
                                        Consultar
                                    </button>
                                    @*create-pages results="ProveedorEti2" number-per-page="10" ng-init="CargaConsulta(); showPaginate=true;" pages="pagesCon"
                                    page-content="ProveedorEti;" page-limit=5000 binding="blur"*@

                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-3 titulos">
                                    <span class="lbl">Todos</span>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-3" style=" width:190px;">
                                    <label>
                                        <input type="radio" class="ace" ng-model="rdbCodigo" name="PorProveedor" value="0" id="rbtArtRef"  />
                                        <span class="lbl">Todos los Proveedores</span>
                                    </label>
                                </div>
                                
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-3 titulos">
                                    <span class="lbl">Código Proveedor</span>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2" style=" width:150px;">
                                    <label>
                                        <input type="radio" class="ace" ng-model="rdbCodigo" name="PorProveedor" value="1" id="rbtArtRef1"  />
                                        <span class="lbl">Código Proveedor:</span>
                                    </label>
                                </div>
                                <div class="col-md-1">
                                    <input id="form-field-icon-2" maxlength="20" ng-model="txtCodProveedor" ng-pattern="/^\d+$/" name="anim" class="my-input" style="width: 100px; padding: 0 0 0 0; margin: 0 0 0 0;" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" />
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-3 titulos">
                                    <span class="lbl">RUC</span>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2" style=" width:150px;">
                                    <label>
                                        <input type="radio" class="ace" ng-model="rdbCodigo" name="PorProveedor" value="2"  id="rbtArtRef2" />
                                        <span class="lbl">RUC:</span>
                                    </label>
                                </div>
                                <div class="col-md-1">
                                    <input id="form-field-icon-2" maxlength="13" ng-model="txtRuc" ng-pattern="/^\d+$/" name="anim" class="my-input" style="width: 100px; padding: 0 0 0 0; margin: 0 0 0 0;" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" />
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-3 titulos">
                                    <span class="lbl">Nombre Comercial</span>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2" style=" width:190px;">
                                    <label>
                                        <input type="radio" class="ace" ng-model="rdbCodigo" name="PorProveedor" value="3" id="rbtArtRef3" />
                                        <span class="lbl">Nombre Comercial:</span>
                                    </label>
                                </div>

                            </div>
                            <div class="row">

                                <div class="col-md-1">
                                    <input id="form-field-icon-2" maxlength="50" ng-model="txtNomComer" name="anim" class="my-input" style="width: 250px; padding: 0 0 0 0; margin: 0 0 0 0;" />
                                </div>
                            </div>


                            <br />
                            <br />
                        </div>
                 
                </div>
            </div>
            
        </div>
    </div>
    @*Fin Session Busqueda*@



    @*Session Botones*@


    @*Fin Session Botones*@
    @*Inicio Session Tabs*@
    <div cg-busy="{promise:myPromise,message:message,backdrop:true}">
        <div class="tabbable">
            <ul class="nav nav-tabs" id="myTab">

                <li class="active">
                    <a data-toggle="tab" href="#contactoproveedor">
                       Asignación de Proveedor
                    </a>
                </li>
            </ul>
            <div class="col-md-3 col-xs-3 col-lg-3" style="text-align: right;max-width:1px;max-width:1px; opacity:0;">
                <input type="radio" name="FiltrosCons" id="rbtPorUsuario" ng-model="rbtPorUsuario" value=""
                       create-pages results="ProveedorEti2" number-per-page="30" ng-init="CargaConsulta(); showPaginate=true;" pages="pagesCon"
                       page-content="ProveedorEti;" page-limit=9000 binding="blur" />


            </div>
            <input type="radio" name="pruebaNot" id="cargaNotificacion" style=" max-width:1px;max-width:1px; opacity:0;" />
            <div class="tab-content">

                <!-- #section:Contacto Datos del Proveedor -->
                <div id="contactoproveedor" class="tab-pane fade in active" @*ng-disabled="isdisGerente"*@>

                    <div class="widget-box">
                        <div class="widget-header" style="text-align:center;">
                            <h4 class="widget-title">
                                BANDEJA DE ASIGNACIÓN PROVEEDOR/ETIQUETAS
                            </h4>
                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>
                            </div>
                            <font ng-hide="etiTotRegistros==''" class="widget-toolbar"> Total: {{etiTotRegistros}} registros </font>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main">
                                <div class="widget-box">


                                    <div class="widget-body">
                                        <div class="widget-main">

                                            <div class="row">
                                                <div class="col-md-12 col-xs-12 col-lg-12">
                                                    <div class="widget-body">
                                                        <div class="widget-main" style="padding:0 0 0 0;">

                                                            <div ng-controller="PaginationController">
                                                                <div ng-show="showPaginate">
                                                                    <div id="no-more-tables">
                                                                        <table class="table table-striped table-bordered table-condensed table-hover gridStyle">
                                                                            <thead>
                                                                                <tr>

                                                                                    <th class="grid-header uppercase info" st-sort="CodProveedor" style="font-size:11px; ">
                                                                                        <a href="#" ng-click="sortType = 'CodProveedor'; sortReverse = !sortReverse">
                                                                                            Código Proveedor
                                                                                            <span ng-show="sortType == 'CodProveedor' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                            <span ng-show="sortType == 'CodProveedor' && sortReverse" class="fa fa-caret-up"></span>
                                                                                        </a>
                                                                                    </th>
                                                                                    <th class="grid-header uppercase info" st-sort="Ruc" style="font-size:11px;">

                                                                                        <a href="#" ng-click="sortType = 'Ruc'; sortReverse = !sortReverse">
                                                                                            RUC
                                                                                            <span ng-show="sortType == 'Ruc' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                            <span ng-show="sortType == 'Ruc' && sortReverse" class="fa fa-caret-up"></span>
                                                                                        </a>

                                                                                    </th>
                                                                                    <th class="grid-header uppercase info" st-sort="NombreComercial" style="font-size:11px; ">

                                                                                        <a href="#" ng-click="sortType = 'NombreComercial'; sortReverse = !sortReverse">
                                                                                            Nombre Comercial
                                                                                            <span ng-show="sortType == 'NombreComercial' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                            <span ng-show="sortType == 'NombreComercial' && sortReverse" class="fa fa-caret-up"></span>
                                                                                        </a>
                                                                                    </th>

                                                                                    <th class="grid-header uppercase info" st-sort="GeneraE" style="font-size:11px; text-align:center">

                                                                                        <a href="#" ng-click="sortType = 'GeneraE'; sortReverse = !sortReverse">
                                                                                            Genera Etiquetas
                                                                                            <span ng-show="sortType == 'GeneraE' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                            <span ng-show="sortType == 'GeneraE' && sortReverse" class="fa fa-caret-up"></span>
                                                                                        </a>
                                                                                    </th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <tr ng-repeat="content in ProveedorEti">
                                                                                    <td style="font-size:11px;text-align:center" data-title="Código Proveedor">{{content.codProveedor}}</td>
                                                                                    <td style="font-size:11px;text-align:center" data-title="RUC">{{content.ruc}}</td>
                                                                                    <td style="font-size:11px;" data-title="Nombre Comercial">{{content.nombreComercial}}  </td>
                                                                                    <td style="font-size:11px;text-align:center" data-title="Genera Etiquetas">
                                                                                        <label>
                                                                                            <input id="rdlreplegal" ng-disabled="isDisabledApr" checked="checked" type="checkbox" ng-model="content.generaEtiqueta" class="ace ace-switch ace-switch-2" ng-click="GeneraEtiqueta(content)" disabled />
                                                                                            <span class="lbl middle"></span>
                                                                                        </label>
                                                                                    </td>
                                                                                </tr>
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                    <ul style="position: relative;" class="pagination" ng-hide="pagesCo.length==0">

                                                                        <li pagination ng-class="[{active: page == getCurrentPage()}, {disabled: isFirstPage()} ]  " page="1">
                                                                            <a><div class="glyphicon glyphicon-fast-backward"></div></a>
                                                                        </li>
                                                                        <li ng-class="{disabled: isFirstPage()}" pagination-left><a><div class="glyphicon glyphicon-backward"></div></a></li>
                                                                        <li>
                                                                            <a> {{getCurrentPage()}} </a> <a>de</a> <a> {{pagesCon.length}}</a>
                                                                        </li>
                                                                        <li ng-class="{disabled: isLastPage()}" pagination-right><a><div class="glyphicon glyphicon-forward"></div></a></li>
                                                                        <li pagination ng-class="[{active: page == getCurrentPage()}, {disabled: isLastPage()} ]  " page="{{pagesCon.length}}">
                                                                            <a><div class="glyphicon glyphicon-fast-forward"></div></a>
                                                                        </li>
                                                                    </ul>

                                                                    @*<ul style="position: relative;" class="pagination">
                                                                        <li ng-class="{disabled: isFirstPage()}" pagination-left><a>&laquo;</a></li>
                                                                        <li pagination ng-class="{active: page == getCurrentPage() }" page="{{page}}" ng-repeat="page in pagesCon">
                                                                            <a>{{page}}</a>
                                                                        </li>
                                                                        <li ng-class="{disabled: isLastPage()}" pagination-right><a>&raquo;</a></li>
                                                                    </ul>*@
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>


                                            @Html.Partial("~/Views/Shared/_mensajeError.cshtml")
                                            @Html.Partial("~/Views/Shared/_mensajeOK.cshtml")
                                            @Html.Partial("~/Views/Shared/_mensajeInformativo.cshtml")
                                            @Html.Partial("~/Views/Shared/_mensajeConfirmacionEliminar.cshtml")
                                            @Html.Partial("~/Views/Shared/_mensajeConfirmacion.cshtml")


                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- #section:fin Contacto Datos del Proveedor -->

            </div>
        </div>
    </div>









</body>

    


