@{
    ViewBag.Title = "Registro de Nuevo Usuario";
    Layout = "~/Views/Shared/_LayoutPrivado.cshtml";
}
@section styles{
    <link href="../assets/css/angular-busy.css" rel="stylesheet" />
}
<body data-ng-controller="frmUsrFirstLogonController">

    <div cg-busy="{promise:myPromise,message:message,backdrop:true}">

        <form name="frmPaso1" ng-submit="btnSiguienteP1()">

            <div class="modal fade" data-backdrop="static" data-keyboard="false" id="UsrFirstLogonP1Dialog" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%; z-index: 50;top:20px">
                <div class="modal-dialog" style="height:40%; width:50%;">
                    <div class="modal-content">
                        <div class="modal-header" style="text-align:left;">
                            <h2>{{varTitulo}}</h2>
                            <h4><strong>Paso 1: Inicio</strong></h4>
                            <br />
                            <div style="text-align: justify">
                                Estimado(a) <strong>{{usrValida.pNombre}}</strong> gracias por colaborar con nuestro
                                proceso de activación de nuevos usuarios.
                                <br />
                                <br />
                                Identificamos su ingreso con el usuario <strong>{{usrValida.pUsuario}}</strong>
                                asociado al RUC de Proveedor <strong>{{usrValida.pRuc}}</strong>.
                                <br />
                                <br />
                                Con el fin de completar la activación de su nuevo usuario
                                es necesario que concluya los siguientes pasos:
                                <ul>
                                    <li>Confirmación de datos de validación</li>
                                    <li>Actualización de información de contacto</li>
                                    <li>Selección de imagen de seguridad e ingreso de respuestas a preguntas de seguridad</li>
                                    <li>Cambio de Contraseña</li>
                                </ul>
                                Una vez finalizados podrá ingresar a nuestro portal.
                            </div>
                        </div>
                        <div class="modal-footer">

                            <div class="row">
                                <div class="col-md-12" style="text-align: center;">
                                    <button type="button" id="btnCerrarP1" ng-click="btnCancelar()" class="btn btn-sm btn-round btn-primary" data-dismiss="modal" aria-hidden="true" style="font-size:10pt">
                                        <i class="ace-icon fa fa-close"></i>
                                        Cancelar
                                    </button>
                                    <button type="submit" id="btnSiguienteP1" class="btn btn-sm btn-round btn-primary" style="font-size:10pt;margin-left:50px;">
                                        <i class="ace-icon fa fa-arrow-right"></i>
                                        Siguiente
                                    </button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </form>

        <form name="frmPaso2" ng-submit="btnSiguienteP2()">

            <div class="modal fade" data-backdrop="static" data-keyboard="false" id="UsrFirstLogonP2Dialog" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%; z-index: 50;top:20px">
                <div class="modal-dialog" style="height:50%; width:50%;">
                    <div class="modal-content">
                        <div class="modal-header" style="text-align:left;">
                            <h2>{{varTitulo}}</h2>
                            <h4><strong>Paso 2: Confirmación de datos de validación</strong></h4>
                            A continuación deber confirmar los datos del proveedor que fueron enviados en el correo de notificación:
                        </div>
                        <div class="modal-body" style="max-height: 50%; overflow-y: auto;">
                            <div class="panel-body">

                                <div class="row">
                                    <div class="col-md-5">
                                        <label class="cls-label">Código de Proveedor:</label>
                                    </div>
                                    <div class="col-md-7">
                                        <input type="text" id="txtCodSAP" ng-model="usrValida.pCodSAP" maxlength="10" placeholder="Código" class="col-md-12 form-control" ng-required="true" oninvalid="setCustomValidity('Código de Proveedor es requerido')" oninput="setCustomValidity('')" />
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-5">
                                        <label class="cls-label">Identificación del Representante Legal:</label>
                                    </div>
                                    <div class="col-md-7">
                                        <input type="text" id="txtIdentificacion" ng-model="usrValida.pIdentificacion" maxlength="13"  placeholder="Identificación" class="col-md-12 form-control" ng-required="true" oninvalid="setCustomValidity('Identificación es requerida')" oninput="setCustomValidity('')" />
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-5">
                                        <label class="cls-label">Correo Electrónico:</label>
                                    </div>
                                    <div class="col-md-7">
                                        <input type="email" id="txtCorreoE" ng-model="usrValida.pCorreoOrig" maxlength="50" placeholder="Correo Electrónico" class="col-md-12 form-control" ng-required="true" oninvalid="setCustomValidity('Correo Electrónico es requerido')" oninput="setCustomValidity('')" />
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-5">
                                    </div>
                                    <div class="col-md-7">
                                        <input type="text" id="txtCAPTCHA" name="txtCAPTCHA" maxlength="4" placeholder="Cod.Captcha" class="form-control" ng-required="true" oninvalid="setCustomValidity('Código CAPTCHA es requerido')" oninput="setCustomValidity('')">
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        &nbsp;
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="modal-footer">

                            <div class="row">
                                <div class="col-md-12" style="text-align: center;">
                                    <button type="button" id="btnCerrarP2" class="btn btn-sm btn-primary btn-round" ng-click="btnCancelar()" data-dismiss="modal" aria-hidden="true" style="font-size:10pt;">
                                        <i class="ace-icon fa fa-close"></i>
                                        Cancelar
                                    </button>
                                    <button type="submit" id="btnSiguienteP2" class="btn btn-sm btn-primary btn-round" style="font-size:10pt;margin-left:50px;">
                                        <i class="ace-icon fa fa-arrow-right"></i>
                                        Siguiente
                                    </button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </form>

        <form name="frmPaso3" ng-submit="btnSiguienteP3()">

            <div class="modal fade" data-backdrop="static" data-keyboard="false" id="UsrFirstLogonP3Dialog" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%; z-index: 50;top:20px">
                <div class="modal-dialog" style="height:50%; width:60%;">
                    <div class="modal-content">
                        <div class="modal-header" style="text-align:left;">
                            <h2>{{varTitulo}}</h2>
                            <h4><strong>Paso 3: Actualización de INFORMACIÓN DE CONTACTO</strong></h4>
                            Debe confirmar o cambiar la siguiente información de contacto para su usuario:
                        </div>
                        <div class="modal-body" style="max-height: 50%; overflow-y: auto;">
                            <div class="panel-body">

                                <div class="row">
                                    <div class="col-md-7">
                                        <label class="cls-label">Correo Electrónico:</label><br />
                                        <div class="cls-label-info">
                                            LOS CONTROLES DE VALIDACIÓN SE ACTIVAN SÓLO SI CAMBIA EL CORREO MOSTRADO
                                            <br /><strong>(OMITIR PARA MANTENER EL MISMO CORREO)</strong>
                                            <ul>
                                                <li>Click en "Enviar Código de Validación" (válido por 10 minutos)</li>
                                                <li>Revisar en su correo el Código enviado</li>
                                                <li>Ingresar el Código en el campo de validación</li>
                                                <li>Click en "Validar Código Ingresado"</li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-md-5">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <input type="email" id="txtCorreoE" ng-model="usrIng.pCorreoE" maxlength="50" placeholder="Correo Electrónico" class="col-md-12" ng-required="true" oninvalid="setCustomValidity('Correo Electrónico es requerido')" oninput="setCustomValidity('')" />
                                            </div>
                                            <div class="col-md-12">
                                                <button type="button" id="btnEnvCodVal" class="btn btn-sm btn-round btn-primary col-md-10" style="font-size:10pt;" ng-click="btnEnvCodValClick()" >
                                                    <i class="ace-icon fa fa-send"></i>
                                                    Enviar Código de Validación
                                                </button>
                                            </div>
                                            <div class="col-md-12">
                                                <input type="text" id="txtCodValidaCorreoE" class="col-md-7" ng-model="usrValida.pCodValidacionIng" maxlength="4" only-digits placeholder="Código Validación" ng-disabled="usrValida.pCodigoOK" />
                                            </div>
                                            <div class="col-md-12">
                                                <button type="button" id="btnCompCodVal" class="btn btn-sm btn-round btn-primary col-md-10" style="font-size:10pt;" ng-click="btnCompCodValClick()" ng-disabled="usrValida.pCodigoOK">
                                                    <i class="ace-icon fa fa-book"></i>
                                                    Validar Código Ingresado
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-7">
                                        <label class="cls-label">Celular:</label>
                                    </div>
                                    <div class="col-md-5">
                                        <input type="text" id="txtCelular" ng-model="usrIng.pCelular" maxlength="10" only-digits placeholder="Celular" class="col-md-12 form-control" ng-required="true" oninvalid="setCustomValidity('Celular es requerido')" oninput="setCustomValidity('')" />
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-7">
                                        <label class="cls-label">Teléfono:</label>
                                    </div>
                                    <div class="col-md-5">
                                        <input type="text" id="txtTelefono" ng-model="usrIng.pTelefono" maxlength="20" only-digits placeholder="Teléfono" class="col-md-12 form-control" oninvalid="setCustomValidity('Teléfono es requerido')" oninput="setCustomValidity('')" />
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="modal-footer">

                            <div class="row">
                                <div class="col-md-12" style="text-align: center;">
                                    <button type="button" id="btnCerrarP3" class="btn btn-sm btn-primary btn-round" ng-click="btnCancelar()" data-dismiss="modal" aria-hidden="true" style="font-size:10pt;">
                                        <i class="ace-icon fa fa-close"></i>
                                        Cancelar
                                    </button>
                                    <button type="submit" id="btnSiguienteP3" class="btn btn-sm btn-primary btn-round" style="font-size:10pt;margin-left:50px;">
                                        <i class="ace-icon fa fa-arrow-right"></i>
                                        Siguiente
                                    </button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </form>

        <form name="frmPaso4" ng-submit="btnSiguienteP4()">

            <div class="modal fade" data-backdrop="static" data-keyboard="false" id="UsrFirstLogonP4Dialog" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%; z-index: 50;top:20px">
                <div class="modal-dialog" style="height:50%; width:50%;">
                    <div class="modal-content">
                        <div class="modal-header" style="text-align:left;">
                            <h2>{{varTitulo}}</h2>
                            <h4><strong>Paso 4: Selección de información de seguridad</strong></h4>
                        </div>
                        <div class="modal-body" style="max-height: 50%; overflow-y: auto;">
                            <div class="panel-body">

                                <div id="accordion" class="accordion-style1 panel-group">
                                    <div class="panel panel-default">

                                        <div class="panel-heading">
                                            <h4 class="panel-title">
                                                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapse_01">
                                                    <i class="ace-icon fa fa-angle-down bigger-110" data-icon-hide="ace-icon fa fa-angle-down" data-icon-show="ace-icon fa fa-angle-right"></i>
                                                    &nbsp;Seleccione una Imagen de Seguridad:
                                                </a>
                                            </h4>
                                        </div>

                                        <div class="panel-collapse collapse in" id="collapse_01">
                                            <div class="panel-body">

                                                <div class="row">
                                                    <div class="col-md-9">
                                                        <div ng-repeat="item in usrValida.listImgs | orderBy:'-sec'" ng-click="toggleImgSelected(item)" ng-class="{'selected':item.imageUrl == imgSelected}" class="col-md-3">
                                                            <img ng-src="{{item.imageUrl}}" class="cls-img-ufl" />
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3" style="text-align: center;">
                                                        <label>Imagen Seleccionada:</label><br />
                                                        <img ng-src="{{imgSelected}}" class="cls-img-sel-ufl" alt="Imagen Seleccionada" />
                                                    </div>
                                                </div>
                                                
                                            </div>
                                        </div>
                                        
                                        <div class="panel-heading">
                                            <h4 class="panel-title">
                                                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapse_02">
                                                    <i class="ace-icon fa fa-angle-down bigger-110" data-icon-hide="ace-icon fa fa-angle-down" data-icon-show="ace-icon fa fa-angle-right"></i>
                                                    &nbsp;Ingrese la respuesta para al menos 3 Preguntas de Seguridad
                                                </a>
                                            </h4>
                                        </div>

                                        <div class="panel-collapse collapse" id="collapse_02">
                                            <div class="panel-body">

                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="widget-body">
                                                            <div class="widget-main">
                                                                <table class="table table-striped table-bordered table-condensed table-hover gridStyle">
                                                                    <thead>
                                                                        <tr>
                                                                            <th class="grid-header uppercase info">Preguntas</th>
                                                                            <th class="grid-header uppercase info">Respuestas (mínimo 3)</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr ng-repeat="row in usrIng.listPregs | orderBy:'+pCodigo'">
                                                                            <td style="font-size:smaller;">{{row.pPregunta}}</td>
                                                                            <td style="font-size:smaller;">
                                                                                <input type="text" id="txtRespSecreta" ng-model="row.pRespuesta" maxlength="50" placeholder="Respuesta" style="width:100%" class="valIngText1" />
                                                                            </td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                        
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="modal-footer">

                            <div class="row">
                                <div class="col-md-12" style="text-align: center;">
                                    <button type="button" id="btnCerrarP4" ng-click="btnCancelar()" class="btn btn-sm btn-primary btn-round" data-dismiss="modal" aria-hidden="true" style="font-size:10pt;">
                                        <i class="ace-icon fa fa-close"></i>
                                        Cancelar
                                    </button>
                                    <button type="submit" id="btnSiguienteP4" class="btn btn-sm btn-primary btn-round" style="font-size:10pt;margin-left:50px;">
                                        <i class="ace-icon fa fa-arrow-right"></i>
                                        Siguiente
                                    </button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </form>

        <form name="frmPaso5" ng-submit="btnSiguienteP5()">

            <div class="modal fade" data-backdrop="static" data-keyboard="false" id="UsrFirstLogonP5Dialog" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%; z-index: 50;top:20px">
                <div class="modal-dialog" style="height:40%; width:50%;">
                    <div class="modal-content">
                        <div class="modal-header" style="text-align:left;">
                            <h2>{{varTitulo}}</h2>
                            <h4><strong>Paso 5: Cambio de Contraseña</strong></h4>
                            Para finalizar debe ingresar su nueva contraseña.
                        </div>
                        <div class="modal-body" style="max-height: 50%; overflow-y: auto;">
                            <div class="panel-body">

                                <div class="row">
                                    <div class="col-md-3" style="padding:0px;">
                                        <label class="cls-label">RUC:</label>
                                    </div>
                                    <div class="col-md-9" style="padding:5px;">
                                        <label class="panel-title">{{usrValida.pRuc}}</label>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-3" style="padding:0px;">
                                        <label class="cls-label">Usuario:</label>
                                    </div>
                                    <div class="col-md-9" style="padding:5px;">
                                        <label class="panel-title">{{usrValida.pUsuario}}</label>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-3" style="padding:0px;">
                                        <label class="cls-label">Nombre:</label>
                                    </div>
                                    <div class="col-md-9" style="padding:5px;">
                                        <label class="panel-title">{{usrValida.pNombre}}</label>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        &nbsp;
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-7">

                                        <div class="row">
                                            <div class="col-md-5" style="padding:0px;">
                                                <label class="cls-label">Nueva Contraseña:</label>
                                            </div>
                                            <div class="col-md-7">
                                                <input type="password" id="txtClaveNew" ng-model="usrIng.pClaveNew" maxlength="20" placeholder="Nueva Contraseña" class="col-md-9 form-control" ng-required="true" oninvalid="setCustomValidity('Nueva Contraseña es requerida')" oninput="setCustomValidity('')" />
                                            </div>
                                        </div>

                                        <br />

                                        <div class="row">
                                            <div class="col-md-5" style="padding:0px;">
                                                <label class="cls-label">Confirmar Contraseña:</label>
                                            </div>
                                            <div class="col-md-7">
                                                <input type="password" id="txtClaveCnf" ng-model="usrIng.pClaveCnf" maxlength="20" placeholder="Nueva Contraseña" class="col-md-9 form-control" ng-required="true" oninvalid="setCustomValidity('Confirmación de Contraseña es requerida')" oninput="setCustomValidity('')" />
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-md-5" style="padding:0px;vertical-align:middle;">
                                        <i class="ace-icon fa fa-lock" style="color:red;margin-left:5px;"></i>&nbsp;&nbsp;<strong>NUEVA CONTRASEÑA</strong>
                                        <spa class="cls-label-info" style="padding:0px;">
                                            <ul>
                                                <li>Mínimo 8 caracteres</li>
                                                <li>Empezar con carácter alfabético</li>
                                                <li>Al menos un carácter en mayúscula</li>
                                                <li>Al menos un carácter en minúscula</li>
                                                <li>Al menos un carácter numérico</li>
                                            </ul>
                                        </spa>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        &nbsp;
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="modal-footer">

                            <div class="row">
                                <div class="col-md-12" style="text-align: center;">
                                    <button type="button" id="btnCerrarP5" ng-click="btnCancelar()" class="btn btn-sm btn-round btn-primary" data-dismiss="modal" aria-hidden="true" style="font-size:10pt;">
                                        <i class="ace-icon fa fa-close"></i>
                                        Cancelar
                                    </button>
                                    <button type="submit" id="btnSiguienteP5" class="btn btn-sm btn-round btn-primary" style="font-size:10pt;margin-left:50px;">
                                        <i class="ace-icon fa fa-save"></i>
                                        Finalizar y Actualizar
                                    </button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </form>
        
        @Html.Partial("_mensajeError")
        @Html.Partial("_mensajeInformativo")
        @Html.Partial("_mensajeOK")

    </div>
</body>
@section scripts{
<script src="../Scripts/general/jquery.plugin.js"></script>
<link href="../Content/jquery.keypad.css" type="text/css" rel="Stylesheet" />
<script src="../Scripts/general/jquery.keypad.js"></script>
<link rel="stylesheet" type="text/css" href="../Content/jquery.realperson.css">
<script src="../Scripts/general/jquery.realperson.js"></script>

<script type="text/javascript">
    $(document).ready(function () {

        $('#txtCodValidaCorreoE').keypad({
            showOn: 'both', buttonImage: '../Images/keypad.png'
            , layout: [
                $.keypad.HALF_SPACE + '12345',
                $.keypad.HALF_SPACE + '67890',
                $.keypad.CLEAR + $.keypad.BACK + $.keypad.CLOSE]
            , randomiseAlphabetic: true, randomiseNumeric: true
        });
        

        $('#txtCodSAP').keypress(function (e) {
            var regex = new RegExp("^[a-zA-Z0-9]");
            var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
            if (regex.test(str)) {
                return true;
            }
            e.preventDefault();
            return false;
        });
        $('#txtCodSAP').keyup(function () {
            this.value = this.value.toUpperCase();
        });

        $('.valIngText1').keypress(function (e) {
            var regex = new RegExp("^[a-zA-Z0-9 _]+$");
            var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
            if (regex.test(str)) {
                return true;
            }
            e.preventDefault();
            return false;
        });

        
        //constante para el CAPTCHA
        $.salt = '123456';//no cambiar
        $('#txtCAPTCHA').keyup(function () {
            this.value = this.value.toUpperCase();
        });
        $('#txtCAPTCHA').realperson({
            length: 4, chars: $.realperson.alphanumeric, dot: 'o', dots:
            [['   *   ', '  ***  ', '  ***  ', ' **  * ', ' ***** ', '**    *', '**    *'],
            ['****** ', '**    *', '**    *', '****** ', '**    *', '**    *', '****** '],
            [' ***** ', '**    *', '**     ', '**     ', '**     ', '**    *', ' ***** '],
            ['****** ', '**    *', '**    *', '**    *', '**    *', '**    *', '****** '],
            ['*******', '**     ', '**     ', '****   ', '**     ', '**     ', '*******'],
            ['*******', '**     ', '**     ', '****   ', '**     ', '**     ', '**     '],
            [' ***** ', '**    *', '**     ', '**     ', '**  ***', '**    *', ' ***** '],
            ['**    *', '**    *', '**    *', '*******', '**    *', '**    *', '**    *'],
            ['*******', '  **   ', '  **   ', '  **   ', '  **   ', '  **   ', '*******'],
            ['     **', '     **', '     **', '     **', '     **', '*    **', ' ***** '],
            ['**    *', '**  ** ', '****   ', '**     ', '****   ', '**  ** ', '**    *'],
            ['**     ', '**     ', '**     ', '**     ', '**     ', '**     ', '*******'],
            ['*     *', '**   **', '*** * *', '** *  *', '**    *', '**    *', '**    *'],
            ['*     *', '**    *', '***   *', '** *  *', '**  * *', '**   **', '**    *'],
