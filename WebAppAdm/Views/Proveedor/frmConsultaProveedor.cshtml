<body class="page_container row ng-cloak" data-ng-controller="ConsultaProveedorController">
    <div id="main-container" >
        <div cg-busy="{promise:myPromise,message:message,backdrop:true}">
            <form name="indexform">
                

                <div class="tabbable">
                    <ul class="nav nav-tabs" id="myTab">
                        <li class="active">
                            <a data-toggle="tab" href="#datosproveedor">
                                Datos del Proveedor
                            </a>
                        </li>
                        
                       
                    </ul>
                    <div class="tab-content">
                        <!-- #section:Datos del Proveedor -->
                        <div id="datosproveedor" class="tab-pane fade in active" @*ng-disabled="isdisGerente"*@>
                            <!--Inicio Identificacion -->
                            <div class="widget-box">
                                <div class="widget-header">
                                    <h4 class="widget-title" style="font-weight:normal">
                                        Identificación

                                    </h4>
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                    <div class="widget-toolbar">
                                        <a href="#" data-action="collapse">
                                            <i class="ace-icon fa fa-chevron-up"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="widget-body">
                                    <div class="widget-main">
                                        <div class="row">
                                            <div class="col-xs-12">
                                                <div class="form-group">

                                                    <label class="col-sm-2 control-label no-padding-right" for="ddlTipoIndentificacion"> Tipo Identificación: </label>

                                                    <div class="col-sm-2">
                                                        <!--<input type="text" id="form-field-1" placeholder="Username" class="col-xs-5 col-sm-5" />-->
                                                        <select name="seletipoidentificacionProveedor" class="col-sm-2 chosen-select form-control" id="ddlTipoIndentificacion" data-ng-model="Ingreso.tipoidentificacion" data-placeholder="Seleccione un Tipo Identificación.." ng-options="item as item.detalle for item in ListTipoIdentificacion"
                                                                ng-required="true" oninvalid="setCustomValidity('Tipo de Identificacion requerido')" oninput="    setCustomValidity('')"
                                                                data-ng-class='{ error: indexform.seletipoidentificacionProveedor.$invalid && !indexform.$pristine }' disabled>
                                                            <option value=""> Seleccione un Tipo Identificación... </option>

                                                        </select>

                                                    </div>

                                                    <label class="col-sm-2 control-label" for="txtIdentificacion"> Identificación: </label>

                                                    <div class="col-md-6 ">
                                                        <input type="text" name="txtIdentificacion" placeholder="Identificación" class="col-xs-10 col-sm-5 no-padding" ng-model="p_SolProveedor.Identificacion" maxlength="13" disabled onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;"
                                                               ng-required="true" oninvalid="setCustomValidity('La Identificacion requerido')" oninput="    setCustomValidity('')" minlength="10" data-ng-class='{ error: indexform.txtIdentificacion.$invalid && !txtIdentificacion.$pristine }' />


                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12 ">
                                                <div class="form-group">
                                                    <label class="col-md-2 control-label no-padding-right" for="txtNombreSri"> Nombre SRI:*</label>

                                                    <div class="col-md-10">

                                                        <input type="text" name="txtNombreSri" ng-model="p_SolProveedor.RazonSocial" maxlength="100" placeholder="Nombre SRI" ng-disabled="isDisabledApr" class="form-control col-md-12 col-md-12 clsToUpper"
                                                               ng-required="true" oninvalid="setCustomValidity('El Nombre SRI requerido')" oninput="    setCustomValidity('')" minlength="4" data-ng-class='{ error: indexform.txtNombreSri.$invalid && !txtNombreSri.$pristine }' />

                                                    </div>


                                                </div>
                                            </div>
                                            <div class="col-md-12 ">
