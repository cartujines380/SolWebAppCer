                                                                                            <input id="rdlreplegal" ng-disabled="true" checked="checked" type="checkbox" ng-model="content.RepLegal" class="ace ace-switch ace-switch-2" disabled />
                                                                                            <span class="lbl middle"></span>
                                                                                        </label>

                                                                                    </td>
                                                                                    <td style="font-size:11px; text-align:center;" data-title="RECIBEACTA"><a class="red" href="#" ng-show="content.pRecActas"><i class="ace-icon glyphicon glyphicon-ok bigger-130"></i></a></td>
                                                                                    <td style="font-size:11px; text-align: center;" data-title="Estado" ng-show="content.Estado"><b>A</b></td>
                                                                                    <td style="font-size:11px; text-align: center;" data-title="Estado" ng-show="!content.Estado"><b>I</b></td>


                                                                                    


                                                                                    <td style="font-size:11px; text-align:center" data-title="Seleccionar">
