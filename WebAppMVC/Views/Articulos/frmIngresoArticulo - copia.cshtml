                        </div>
                        <div class=" col-md-2" style="text-align: right;">
                            <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important"
                                    id="btnAddImagen" ng-click="RechazarArticulo()">
                                <i class="glyphicon glyphicon-remove "></i>
                                Rechazar Artículo
                            </button>
                        </div>
                        <div class="col-md-2" style="text-align:right;">
                            <button type="submit" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important"
                                    id="btnEnviar" ng-click="NextObservaciones()" data-ng-disabled='!requeridodet.$valid'>
                                <i class="glyphicon glyphicon-play-circle"></i>
                                Siguiente
                            </button>
                        </div>
                    </div>

                </div>
                <div id="Observaciones" ng-hide="false" class="tab-pane fade">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="widget-title">Observaciones</h4>
                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>

                            </div>
                        </div>

                        <div class="widget-body">
                            <div class="widget-main">
                                <div style=" height: 200px !important;overflow: scroll;">
                                    <div id="no-more-tables">
                                        <table class="nu-table table gridStyle">
                                            <thead>
                                                <tr>
                                                    <th class="grid-header uppercase info" st-sort="fecha">Fecha</th>
                                                    <th class="grid-header uppercase info" st-sort="estadoSolicitud">Estado Solicitud</th>
                                                    <th class="grid-header uppercase info" st-sort="usuario">Usuario</th>
                                                    <th class="grid-header uppercase info" st-sort="motivo">Motivo</th>
                                                    <th class="grid-header uppercase info" st-sort="observacion">Observación</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="content in grArtObservacion">
                                                    <td style="font-size:smaller" data-title="Código">{{content.fecha}}</td>
                                                    <td style="font-size:smaller" data-title="Estado Solicitud">{{content.estadoSolicitud}}</td>
                                                    <td style="font-size:smaller" data-title="Usuario">{{content.usuario}}</td>
                                                    <td style="font-size:smaller" data-title="Motivo">{{content.motivo}}</td>
                                                    <td style="font-size:smaller" data-title=" Observación">{{content.observacion}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
        <!-- ******************** Carga Masiva *********************-->
        <div class="modal fade" data-backdrop="static" data-keyboard="false" id="idCargaMasiva" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%;">
            <div class="modal-dialog" style="height:80%; width:30%;">
                <div class="modal-content">
                    <div class="modal-header" style="text-align:center;">
                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="bigger">
                                    Carga Masiva
                                </h4>
                            </div>
                        </div>
                    </div>
                    <div class="modal-body" style="max-height: 30%; overflow-y: auto;">
                        <div class="panel-body">
                            <div id="MensajeError" class="alert alert-block alert-success">
                                <div nv-file-drop="" uploader="uploader3">
                                    <input type="file" nv-file-select="" uploader="uploader3" />
                                </div>
                                <br />
                            </div>
                            <div style="text-align:center;">
                                <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important"
                                        data-dismiss="modal" id="btnAceptar" ng-click="CargaMasiva()">
                                    <i class="ace-icon fa fa-check"></i>
                                    Aceptar
                                </button>
                                <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important"
                                        data-dismiss="modal" id="btnCancelar">
                                    <i class="glyphicon glyphicon-remove"></i>
                                    Cancelar
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer" style="height:10px">

                    </div>
                </div>
            </div>
        </div>
        <!-- ******************** Fin Carga Masiva *********************-->
        <!-- ******************** Muestra Imagen *********************-->
        <div class="modal fade" data-backdrop="static" data-keyboard="false" id="idMuestraImagen" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%;">
            <div class="modal-dialog" style="height:80%; width:40%;">
                <div class="modal-content">
                    <div class="modal-header" style="text-align:center;">
                        <div class="widget-box">
                            <div class="widget-header">
                                <h4 class="bigger">
                                    Visualizador
                                </h4>
                            </div>
                        </div>
                    </div>
                    <div class="modal-body" style="max-height: 40%; overflow-y: auto;">
                        <div class="panel-body">
                            <div id="panel" class="alert alert-info" style=" text-align:center;">
                                <img src="{{imagenurl}}" width="200" />
                                <br />
                            </div>
                            <div style="text-align:center;">
                                <button type="button" class="btn btn-sm btn-round" style="background-color:darkred!important;border-color:darkred !important" data-dismiss="modal" id="btnConsulta">
                                    <i class="ace-icon fa fa-check"></i>
                                    Aceptar
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer" style="height:10px">

                    </div>
                </div>
            </div>
        </div>
        <!-- ******************** Fin Muestra Imagen *********************-->
    <!-- ******************** Mensaje Confirmacion para Articulo *********************-->
    <div class="modal fade" data-backdrop="static" data-keyboard="false" id="idMensajeConfirmacionArt" tabindex="-1" role="dialog" aria-hidden="true" style="height:100%; width:100%;">
        <div class="modal-dialog" style="height:80%; width:30%;">
            <div class="modal-content">
                <div class="modal-header" style="text-align:center;">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="bigger">
                                Confirmacion
                            </h4>
                        </div>
                    </div>
                </div>
                <div class="modal-body" style="max-height: 30%; overflow-y: auto;">
                    <div class="panel-body">
                        <div id="MensajeError" class="alert alert-block alert-success">
                            <strong>
                                <label>
                                    <span class="lbl">{{MsjConfirmacionArt}}</span>
                                </label>
                            </strong>
                            <br />
                            <div ng-hide="ViewMotRechazoArt">
                                <label>
                                    <span class="lbl">Motivo de Rechazo:</span>
                                </label>
                            </div>
                            <div ng-hide="ViewMotRechazoArt">
                                <select id="selectmarca" class="form-control" name="marca" ng-model="ddlMotivoRechazoArt" ng-options="item as item.detalle for item in motivoRechazoArt"
                                        style=" width:100%; ">
                                    <option value="">Seleccione un Motivo de Rechazo</option>
                                </select>
                            </div>
                        </div>
                        <div style="text-align:center;">
                            <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" data-dismiss="modal" id="btnAceptar"
                                    ng-click="AddArticuloAdmin()">
                                <i class="ace-icon fa fa-check"></i>
                                Grabar
                            </button>
                            <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" data-dismiss="modal" id="btnCancelar">
                                <i class="glyphicon glyphicon-remove"></i>
                                Cancelar
                            </button>
                        </div>
                    </div>
                </div>
                <div class="modal-footer" style="height:10px">

                </div>
            </div>
        </div>
    </div>
    <!-- ******************** Fin Mensaje Confirmacion para Articulo *********************-->
        @Html.Partial("_mensajeError")
        @Html.Partial("_mensajeConfirmacion")
        @Html.Partial("_mensajeInformativo")
        @Html.Partial("_mensajeConfirmacionEliminar")
        @Html.Partial("_mensajeConfirmado")
        @Html.Partial("_mensajeOK")
</body>

@section scripts{
    <script src="~/assets/js/fileinput.js"></script>
    <script src="~/assets/js/fileinput.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {

            $("#file-3").fileinput({
                showCaption: false,
                browseClass: "btn btn-sm btn-round",
                fileType: "any"
            });
        });
    </script>
}
