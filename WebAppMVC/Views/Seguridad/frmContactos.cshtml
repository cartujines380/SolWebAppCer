                                                                                                            <a href="#" ng-click="sortType = 'DescFuncion'; sortReverse = !sortReverse">
                                                                                                                FUNCIÓN
                                                                                                                <span ng-show="sortType == 'DescFuncion' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                                                <span ng-show="sortType == 'DescFuncion' && sortReverse" class="fa fa-caret-up"></span>
