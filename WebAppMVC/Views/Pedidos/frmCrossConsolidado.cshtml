                                        @*<input type="date" id="txtFechaIni" ng-model="pFechaIni" maxlength="10" placeholder="Fecha Inicial" class="form-control" style="width:90%" />*@
                                        <input class="form-control date-picker input-mask-date" id="txtFecha" type="text" ng-model="pFecha" maxlength="10" placeholder="Fecha" data-date-format="dd/mm/yyyy" />
                                        <span class=" input-group-addon">
                                            <i class="fa fa-calendar small-1"></i>
                                        </span>
                                    </div>
                                    @*<input type="date" id="txtFecha" ng-model="pFecha" maxlength="10" placeholder="Fecha" class="form-control" style="width:90%" />*@
