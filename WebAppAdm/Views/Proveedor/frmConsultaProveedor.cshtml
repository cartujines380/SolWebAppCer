                                                                ng-required="true" oninvalid="setCustomValidity('El Sector Comercial es  requerido')" oninput="    setCustomValidity('')"
                                                                required data-ng-class='{ error: indexform.selectSectorComercial.$invalid && !indexform.$pristine }'>
