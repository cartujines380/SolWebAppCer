                                                                                                    <tr>
                                                                                                        <th class="grid-header uppercase info" st-sort="departamento" style="font-size:11px; ">
                                                                                                            <a href="#" ng-click="sortType = 'departamento'; sortReverse = !sortReverse">
                                                                                                                DEPARTAMENTO
                                                                                                                <span ng-show="sortType == 'departamento' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                                                <span ng-show="sortType == 'departamento' && sortReverse" class="fa fa-caret-up"></span>
                                                                                                            </a>
                                                                                                        </th>
