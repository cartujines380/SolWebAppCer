        </ul><!-- /.breadcrumb -->
        <!-- #section:basics/content.searchbox -->
        <!-- /section:basics/content.searchbox -->
    </div>
</div>
    <body data-ng-controller="VehiculosProveedorController" >
        <div style="height :100%; width:100%; font-size:12px;zoom:100%;">
            @*Session Busqueda*@
            <div style="height:100%;">
                <div class="ace-settings-container" id="ace-settings-container">
                    <div class="btn btn-app btn-xs btn-warning ace-settings-btn" id="ace-settings-btn">
                        <i class="ace-icon  glyphicon glyphicon-search bigger-130"></i>
                    </div>
                    <div class="ace-settings-box clearfix" id="ace-settings-box">
                        <div class="container container-fluid" style="padding:0 0 0 0">
                            <div class="row">
                                <div class="col-md-3" style="text-align: right;">
                                    <button type="button" class="btn btn-sm btn-round" id="consultagrid" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important;" create-pages results="GridTransporte" number-per-page="20" ng-click="BuscarFiltro();showPaginate=true" pages="pagesCho" page-content="pageContentCho;" page-limit=15 binding="blur">
                                        <i class=" glyphicon glyphicon-search">
                                        </i>
                                        Consultar
                                    </button>
                                </div>
                            </div>
                            <div>
                                <div class="row">
                                    <div class="col-md-3 titulos">
                                        <span class="lbl">Tipos</span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">
                                        <label>
                                            <input type="radio" name="PorTipos" class="ace" value="1" id="rbtTraTN" ng-model="PorTipos" ng-click="limpioCaja()" />
                                            <span class="lbl">Todos los Tipos</span>
                                        </label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">
                                        <label>
                                            <input type="radio" name="PorTipos" class="ace" value="2" id="rbtTraUN" ng-model="PorTipos" ng-click="limpioCaja()" />
                                            <span class="lbl">Tipo Vehículo</span>
                                        </label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">
                                        <select id="selectipo" class="form-control" ng-model="selectedItemTipo" ng-options="item as item.detalle for item in SolicitudTipo">
                                            <option value="">Seleccione un Tipo Vehículo</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-3 titulos">
                                    <span class="lbl">Propietario</span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <label>
                                        <input type="radio" name="PorPropietario" class="ace" value="1" id="rbtPNombre" ng-model="PorPropietario" ng-click="limpioCaja()" />
                                        <span class="lbl">Todos los Propietarios</span>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <label>
                                        <input type="radio" name="PorPropietario" class="ace" value="2" id="rbtPNombre" ng-model="PorPropietario" ng-click="limpioCaja()" />
                                        <span class="lbl">Propietario:</span>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <input type="text" name="Nombre" id="txtPropietario" class="form-control" ng-model="txtPropietario" placeholder="Propietario" style="margin-left: 30px; width: 70%;" />
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-3 titulos">
                                    <span class="lbl">No. Placa</span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <label>
                                        <input type="radio" name="PorPlaca" class="ace" value="1" id="rbtPNombre" ng-model="PorPlaca" ng-click="limpioCaja()" />
                                        <span class="lbl">Todos los Número</span>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <label>
                                        <input type="radio" name="PorPlaca" class="ace" value="2" id="rbtPNombre" ng-model="PorPlaca" ng-click="limpioCaja()" />
                                        <span class="lbl">Número Placa:</span>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <input type="text" name="Nombre" id="txtPlaca" class="form-control" ng-model="txtPlaca" placeholder="Número Placa" style="margin-left: 30px; width: 70%;" maxlength="8" />
                                </div>
                            </div>
                            <br />
                            <div class="row">
                                <div class="col-md-3 titulos">
                                    <span class="lbl">Estados</span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <label>
                                        <input type="radio" name="PorEstados" class="ace" value="1" id="rbtTodosEstados" ng-model="PorEstadosTipo" ng-click="limpioCaja()" />
                                        <span class="lbl">Todos los Estados:</span>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3 ">
                                    <label>
                                        <input type="radio" name="PorEstados" class="ace" value="2" id="rbtPorEstado" ng-model="PorEstadosTipo" ng-click="limpioCaja()" />
                                        <span class="lbl">Por Estado:</span>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <select id="selectestado" class="form-control" ng-model="selectedItemEstado" ng-options="item as item.detalle for item in EstadoSolicitud">
                                        <option value="">Seleccione un Estado</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @*Fin Session Busqueda*@
            <div cg-busy="{promise:myPromise,message:message,backdrop:true}">
                <form name='exampleForm' ng-submit="Confirmargrabar()">
                    <div class="tabbable">
                        <ul class="nav nav-tabs" id="myTab">
                            <li class="active">
                                <a data-toggle="tab" href="#VehiculosRegistrados">
                                    Vehículos Registrados
                                </a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#RegistroVehiculosNuevos">
                                    Registro Vehículos Nuevos
                                </a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#HistorialPedidos">
                                    Historial Cita
                                </a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#HistorialEstados">
                                    Historial Estados
                                </a>
                            </li>
                        </ul>

                        <div class="tab-content">
                            <div id="VehiculosRegistrados" class="tab-pane fade in active">
                                @*Session Botones*@
                               
                                @*Fin Session Botones*@
                                @*Session Grid de Vehiculos Ingresados*@
                                <div class="widget-box">
                                    <div class="widget-header" style="text-align:center;text-wrap:none;">
                                        <h4 class="widget-title">CONSULTA DE VEHÍCULOS</h4>
                                        <div class="widget-toolbar">
                                            <a href="#" data-action="collapse">
                                                <i class="ace-icon fa fa-chevron-up"></i>
                                            </a>

                                        </div>
                                        <font ng-hide="etiTotRegistros==''" class="widget-toolbar"> Total: {{etiTotRegistros}} Registros </font>
                                    </div>

                                    <div class="widget-body">
                                        <div class="widget-main">
                                            <div class="widget-box">
                                                <div class="widget-header">
                                                    <div class="row">
                                                        <div class=" col-md-12 ">
                                                            <h4 class="smaller" style="text-align:right;">
                                                                <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" id="btnConsulta" ng-click="exportar(1)">
                                                                    <i class="fa fa-file-excel-o "></i>
                                                                    Exportar Excel
                                                                </button>
                                                                <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" id="btnConsulta" ng-click="exportar(2)">
                                                                    <i class="fa fa-file-pdf-o"></i>
                                                                    Exporta PDF
                                                                </button>
                                                            </h4>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div ng-controller="PaginationController">
                                                <div ng-show="showPaginate">
                                                    <div class="span3">
                                                        <div id="no-more-tables">
                                                            <table class="table table-striped table-bordered table-condensed table-hover gridStyle">
                                                                <thead>
                                                                    <tr>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'idvehiculo'; sortReverse = !sortReverse">
                                                                                Número Vehículo
                                                                                <span ng-show="sortType == 'idvehiculo' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'idvehiculo' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'numplaca'; sortReverse = !sortReverse">
                                                                                Número Placa
                                                                                <span ng-show="sortType == 'numplaca' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'numplaca' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'tipovehiculo'; sortReverse = !sortReverse">
                                                                                Tipo Vehículo
                                                                                <span ng-show="sortType == 'tipovehiculo' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'tipovehiculo' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'marca'; sortReverse = !sortReverse">
                                                                                Marca
                                                                                <span ng-show="sortType == 'marca' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'marca' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'modelo'; sortReverse = !sortReverse">
                                                                                Modelo
                                                                                <span ng-show="sortType == 'modelo' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'modelo' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'estado'; sortReverse = !sortReverse">
                                                                                Estado
                                                                                <span ng-show="sortType == 'estado' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'estado' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'colorprincipal'; sortReverse = !sortReverse">
                                                                                Color Principal
                                                                                <span ng-show="sortType == 'colorprincipal' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'colorprincipal' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'colorsecundario'; sortReverse = !sortReverse">
                                                                                Color Secundario
                                                                                <span ng-show="sortType == 'colorsecundario' && !sortReverse" class="fa fa-caret-down"></span>
                                                                                <span ng-show="sortType == 'colorsecundario' && sortReverse" class="fa fa-caret-up"></span>
                                                                            </a>
                                                                        </th>
                                                                        <th class="grid-header uppercase info" style="font-size:11px;">
                                                                            <a href="#" ng-click="sortType = 'colorsecundario'; sortReverse = !sortReverse">
                                                                                Selecccionar
                                                                            </a>
                                                                        </th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="content in pageContentCho  | orderBy:sortType:sortReverse">
                                                                        <td style="font-size:11px;text-align:center" data-title="Número Vehículo">{{content.idvehiculo}}</td>
                                                                        <td style="font-size:11px;" data-title="Número Placa">{{content.numplaca}}</td>
                                                                        <td style="font-size:11px;" data-title="Tipo Vehículo">{{content.tipovehiculo}}</td>
                                                                        <td style="font-size:11px;" data-title="Marca">{{content.marca}}</td>
                                                                        <td style="font-size:11px;" data-title="Modelo">{{content.modelo}}</td>
                                                                        <td style="font-size:11px;" data-title="Estado">{{content.estado}}</td>
                                                                        <td style="font-size:11px;" data-title="Color Principal">{{content.colorprincipal}}</td>
                                                                        <td style="font-size:11px;" data-title="Color Secundario">{{content.colorsecundario}}</td>
                                                                        <td style="font-size:11px;text-align:center" data-title="Selecccionar">
                                                                            <a class="btn btn-danger btn-xs" ng-click="SelecionarGrid(content.idvehiculo);" style="border-width: 1px;padding: 0 5px;"><i class=" glyphicon glyphicon-ok "></i></a>
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                    <ul style="position: relative;" class="pagination">
                                                        <li ng-class="{disabled: isFirstPage()}" pagination-left><a>&laquo;</a></li>
                                                        <li pagination ng-class="{active: page == getCurrentPage() }" page="{{page}}" ng-repeat="page in pagesCho">
                                                            <a>{{page}}</a>
                                                        </li>
                                                        <li ng-class="{disabled: isLastPage()}" pagination-right><a>&raquo;</a></li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*Fin Session Grid de Vehiculos Ingresados*@
                            </div>
                            <div id="RegistroVehiculosNuevos" class="tab-pane fade">
                                @*Session Botones*@
                                <div class="widget-box">
                                    <div class="widget-header">
                                        <h4 class="smaller" style="text-align:right;">
                                            <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" id="btnnuevo" ng-click="nuevo()">
                                                <i class="glyphicon glyphicon-file"></i>
                                                Nuevo
                                            </button>
                                            <button type="submit" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" id="btnConsulta">
                                                <i class="glyphicon glyphicon-floppy-disk"></i>
                                                Grabar
                                            </button>
                                            <br />
                                        </h4>
                                    </div>
                                </div>
                                @*Fin Session Botones*@

                                @*Session Mantenimiento*@
                                <div class='form-group'>
                                    <div class="widget-box">
                                        <div class="widget-header" style="text-align:center;text-wrap:none;">
                                            <h4 class="widget-title" style="font-weight:normal!important">Datos Generales</h4>
                                            <div class="widget-toolbar">
                                                <a href="#" data-action="collapse">
                                                    <i class="ace-icon fa fa-chevron-up"></i>
                                                </a>
                                            </div>
                                        </div>
                                        <div class="widget-body">
                                            <div class="widget-main">
                                                <div class="row">
                                                    <div class=" col-md-9 ">
                                                        <div class="">
                                                            <br />
                                                            <div class="row">
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">No. Placa:*</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <input type="text" name="txtplaca" pattern="[A-Za-z]{3}[0-9]{3,4}" required class="form-control clsToUpper" ng-model="traCho.txtplaca" minlength="6" maxlength="7" placeholder="No. Placa" style=" width: 100%" ng-required="true" oninvalid="setCustomValidity('Placa Vehículo es requerido.')" oninput="    setCustomValidity('')" />
                                                                </div>
                                                                <div class=" col-md-1">
                                                                    <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important;" ng-click="buscarProveedor(3)">
                                                                        <i class=" glyphicon glyphicon-search">
                                                                        </i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">Tipo Identificación:*</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <select id="idselectIdentificacion" name="selectedidentificacion" class="form-control" ng-model="selectedIdentificacion" ng-options="item as item.detalle for item in IdentificacionDatos"
                                                                            ng-required="true" oninvalid="setCustomValidity('Tipo Identificación es requerido.')" oninput="    setCustomValidity('')" style=" width: 100%">
                                                                        <option value="">Seleccione una identificación</option>
                                                                    </select>
                                                                </div>
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">No. Identificación:*</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <input id="txtidentificacion" type="text" name="txtidentificacion"  class="form-control clsToUpper" ng-model="traCho.txtIdentificacion" placeholder="No. Identificación" style=" width: 100%" 
                                                                           minlength="1" maxlength="13" ng-required="true"  oninvalid="setCustomValidity('La Identificación es requerido o No Cumple el minimo Requerido')" oninput="    setCustomValidity('')" />
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">Primer Nombre:*</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <input type="text" name="nombresprimero" class="form-control clsToUpper" maxlength="20" ng-model="traCho.txtNombresPrimero" placeholder="1er Nombre" style=" width: 100%" ng-required="true"
                                                                           oninvalid="setCustomValidity('Primer Nombre es requerido')" oninput="setCustomValidity('')" />
                                                                </div>
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">Segundo Nombre:</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <input type="text" name="Nombres" id="txtNombresSegundo" class="form-control clsToUpper" maxlength="20" ng-model="traCho.txtNombresSegundo" placeholder="2do Nombre" style=" width: 100%" />
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">Primer Apellido:*</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <input type="text" name="apellidoprimero" class="form-control clsToUpper" ng-model="traCho.txtApellidoPrimero" placeholder="1er Apellido" style=" width: 100%" ng-required="true" oninvalid="setCustomValidity('Primer Apellido es requerido.')" oninput="setCustomValidity('')" />
                                                                </div>
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">Segundo Apellido:*</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <input type="text" name="apellidosegundo" class="form-control clsToUpper" maxlength="20" ng-model="traCho.txtApellidoSegundo" placeholder="2do Apellido" style=" width: 100%" ng-required="true" oninvalid="setCustomValidity('Primer Apellido es requerido.')" oninput="setCustomValidity('')" />
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">Dirección Propietario:*</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <input type="text" name="txtdirpropie" class="form-control clsToUpper" maxlength="100" ng-model="traCho.txtdirpropie" placeholder="Dirección Propietario" style=" width: 100%" maxlength="100"
                                                                           ng-required="true" oninvalid="setCustomValidity('La Dirección del Propietario es Requerido.')" oninput="    setCustomValidity('')" />
                                                                </div>
                                                                <div class=" col-md-2">
                                                                    <label>
                                                                        <span class="lbl">Teléfono Propietario:*</span>
                                                                    </label>
                                                                </div>
                                                                <div class=" col-md-4">
                                                                    <input type="text" name="txttelfpro" class="form-control clsToUpper" ng-model="traCho.txttelfpro" placeholder="Teléfono Propietario" minlength="7" maxlength="10" style=" width: 100%" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" 
                                                                           ng-required="true" oninvalid="setCustomValidity('El Teléfono de Porpietario es requerido.')" oninput="    setCustomValidity('')" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <br />
                                                    </div>
