                            <div class="col-md-1">
                                <label>
                                    <span class="lbl">Estado:</span>
                                </label>
                            </div>
                            <div class="col-md-2">
                                <label>
                                    <select class="chosen-select form-control" id="cmbestado" ng-model="cmbestado"
