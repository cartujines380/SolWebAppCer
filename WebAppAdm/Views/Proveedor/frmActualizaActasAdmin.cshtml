                                        <i class="fa fa-file-excel-o "></i>
                                        Exportar XLS
                                    </button>
                                    <button type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" ng-click="exportar(2)">
