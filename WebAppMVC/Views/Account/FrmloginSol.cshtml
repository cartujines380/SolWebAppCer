<div class="login-layout" style="max-width:100% !important" data-ng-controller="indexController">
    @Html.AntiForgeryToken()
    <br />

    <div class="login-containerC" data-ng-controller="loginController" ng-init="validaPestana()">
        <div class="row">
            <center>
                <div cg-busy="{promise:myPromise,message:messageP,backdrop:true}">
                    <div id="login-box" class="login-box visible widget-box no-border" style="color: #666666; font-family: Lato-Regular,Arial, Helvetica, sans-serif; display: table-cell; width: 450px;">
                        <div class="card">
                            <ul class="nav nav-tabs" role="tablist">
                                <li role="presentation" style="width:50%;font-size:16px" class="active">
                                    <a href="#reg" aria-controls="reg" role="tab" data-toggle="tab" style="height: 40px; background-color: #FFF; color: #006865"><p><b>Registro Proveedor</b></p></a>
                                </li>
                                <li role="presentation" style="width:50%;font-size:16px">
                                    <a href="#est" aria-controls="est" role="tab" data-toggle="tab" style="height: 40px; background-color: #FFF; color: #006865"><p><b>Estado Solicitud</b></p></a>
                                </li>
                            </ul>
                            <!-- Tab panes -->
                            <div class="tab-content" style="border: 2px solid #006865 !important;">
                                <div role="tabpanel" class="tab-pane active" id="reg">
                                    <!-- REGISTRO -->
                                    <div data-ng-controller="signupController">
                                        <div cg-busy="{promise:myPromise,message:messageP,backdrop:true}">
                                            <div id="signup-box" style="color: #666666; font-family: Lato-Regular,Arial, Helvetica, sans-serif; display: table-cell; width: 420px;">
                                                <div>
                                                    <p style="line-height: 100%;padding: 8px">
                                                        Si desea ser un nuevo proveedor ingrese sus datos para comenzar el
                                                        proceso o si ya est&aacute; en proceso de calificaci&oacute;n vaya a la pestaña
                                                        "Estado Solicitud"
                                                    </p>
                                                </div>
                                                <form class="form-login" role="form" ng-submit="SignupSolicitante()">
                                                    <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10" style="float:none">
                                                        <label class="block clearfix">
                                                            <span class="block input-icon input-icon-right">
                                                                <input type="text" style="height: 100%" class="form-control" placeholder="RUC" autofocus ng-change="validadorCedula()" minlength="13" autocomplete="off" value="" data-ng-model="registration.ruc" maxlength="13" pattern=".{13,}" ng-required="true" onkeypress="return soloNumeros(event)" autofocusoninvalid="setCustomValidity('RUC es requerido')" oninvalid="setCustomValidity('RUC es Requerido')" oninput="setCustomValidity('')" />
                                                                <i class="ace-icon fa fa-info"></i>
                                                            </span>
                                                        </label>
                                                        <label class="block clearfix">
                                                            <span class="block input-icon input-icon-right">
                                                                <input type="email" style="height: 100%" class="form-control" placeholder="Correo" autocomplete="off" value="" data-ng-model="registration.userName" required ng-required="true" oninvalid="setCustomValidity('Correo es Requerido')" oninput="setCustomValidity('')" />
                                                                <i class="ace-icon fa fa-user fa-envelope"></i>
                                                            </span>
                                                        </label>
                                                        <label class="block clearfix">
                                                            <span class="block input-icon input-icon-right">
                                                                <input type="password" style="height: 100%" class="form-control" placeholder="Contraseña" minlength="6" autocomplete="off" value="" data-ng-model="registration.password" required autofocus ng-required="true" oninvalid="setCustomValidity('Contraseña es Requerida')" oninput="    setCustomValidity('')" />
                                                                <i class="ace-icon fa fa-lock"></i>
                                                            </span>
                                                        </label>
                                                        <label class="block clearfix">
                                                            <span class="block input-icon input-icon-right">
                                                                <input type="password" style="height: 100%" class="form-control" placeholder="Repertir contraseña" minlength="6" data-ng-model="registration.confirmPassword" ng-required="true" autofocus oninvalid="setCustomValidity('Contraseña es Requerida')" oninput="    setCustomValidity('')" />
                                                                <i class="ace-icon fa fa-retweet"></i>
                                                            </span>
                                                        </label>
                                                        <div>
                                                            <input type="text" id="txtCAPTCHA" name="txtCAPTCHA" maxlength="4" placeholder="Código Captcha" ng-required="true">
                                                        </div>
                                                        <div class="space-6"></div>
                                                        <button type="submit" class="btn btn-round btn-primary">
                                                            <span style="font-size:16px"><b>Ingresar</b></span>
                                                        </button>
                                                        <br />
                                                        <button type="reset" class="btn btn-primary" style="background-color:#FFFFFF!important;border-color:#FFF;color:#006865!important">
                                                            <span style="font-size:medium"><u>Limpiar formulario</u></span>
                                                        </button>
                                                        <br />
                                                                    
                                                        <span>
                                                            Copyright 2021 Sipecom S.A.
                                                        </span>
                                                    </div>
                                                    <div data-ng-hide="message == ''" data-ng-class="(savedSuccessfully) ? 'alert alert-success' : 'alert alert-danger'">
                                                        {{message}}
                                                    </div>
                                                </form>
                                            </div>
                                            @Html.Partial("_mensajeRedirect2")
                                        </div>
                                    </div>

                                </div>
                                <div role="tabpanel" class="tab-pane" id="est">
                                    <!-- ESTADO -->
                                    <div>
                                        <p style="line-height: 100%;padding: 8px">
                                            Si ya est&aacute; en proceso de calificaci&oacute;n ingrese sus datos o si es la
                                            primera vez que ingresa y desea ser un nuevo proveedor vaya a la 
                                            pestaña "Registro Proveedor"
                                        </p>
                                    </div>
                                    <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10" style="float:none">
                                        <form class="form-login" name="indexform" role="form" autocomplete="off" ng-submit="login(1)">
                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">
                                                    <input type="text" class="form-control" style="height:100%" placeholder="RUC" autocomplete="off" value="" minlength="13" maxlength="13" pattern=".{13,}" only-digits data-ng-model="loginData.ruc" ng-required="true" onkeypress="return soloNumeros(event)" oninvalid="setCustomValidity('RUC es requerido')" oninput="setCustomValidity('')" autofocus />
                                                    <i class="ace-icon fa fa-info"></i>
                                                </span>
                                            </label>

                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">
                                                    <input type="email" class="form-control" style="height:100%" placeholder="Usuario" autocomplete="off" value="" minlength="3" data-ng-model="loginData.usernamefrm" ng-required="true" oninvalid="setCustomValidity('Usuario es requerido')" oninput="setCustomValidity('')" />
