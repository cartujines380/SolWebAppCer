@{
    ViewBag.Title = "Ingreso Artículo";
    Layout = "~/Views/Shared/_LayoutPrivado.cshtml";
}

@section styles{
    <link href="~/Content/Validation.css" rel="stylesheet" />
    <link href="~/assets/css/fileinput.css" rel="stylesheet" />
}

<div class="breadcrumbs" id="breadcrumbs" style="top:0px;">
    <script type="text/javascript">
        try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
    </script>

    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="~/Home/Index">Inicio</a>
        </li>

        <li>
            <i class="ace-icon fa fa-shopping-cart "></i>
            <a href="~/Home/IndexPrivado">Artículos</a>
        </li>

        <li>
            <i class="ace-icon fa  fa-pencil-square-o"></i>
            <a href="frmIngresoArticulo">Solicitud Modificación Artículo</a>
        </li>
    </ul>
</div>

<body class="page_container row ng-cloak" data-ng-controller="ModArticuloController" style=" font-size:10px;">

    <div class="widget-box">
        <div class="widget-header">
            <div class="row">
                <div class=" col-md-1">
                    <label>
                        <span class="lbl">Línea Negocio:</span>
                    </label>
                </div>
                <div class=" col-md-3">
                    <select ng-disabled="true" id="selectmodelo" class="form-control" name="linea" ng-model="ddlLineaNegocio" ng-options="item as item.detalle for item in lineaNegocio"
                            style=" width: 100%; ">
                        <option value="">Seleccione una Línea de Negocio</option>
                    </select>
                    @*<input ng-disabled="true" type="text" name="Nombres" id="txtLineaNegocio" ng-model="lineaNegocio2" placeholder="Linea de Negocio"  />*@
                </div>


                <div class="col-md-6"></div>
                <div class=" col-md-1" ng-hide="txtIdSolicitud==undefined">
                    <label>
                        <span class="lbl">No. Solicitud:</span>
                    </label>
                </div>
                <div class=" col-md-1" ng-hide="txtIdSolicitud==undefined">
                    <input ng-disabled="true" type="text" class="form-control" name="Nombres" id="txtIdSolicitud" ng-model="txtIdSolicitud" placeholder="Número Solicitud" style=" max-width:150px; " />
                </div>
                @*<div class=" col-md-2">
                        <label>
                            <span class="lbl">Contador Registros Leídos:</span>
                        </label>
                    </div>
                    <div class=" col-md-2">
                        <input ng-disabled="true" type="text" name="Nombres" id="txtContRegistros" ng-model="txtContRegistros" style=" max-width:80px; " />
                    </div>*@

                @*<button ng-hide="inactivaBot" type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important"
                            id="btnCargaMasiva" ng-click="MasivaPopup()">
                        <i class="glyphicon glyphicon-cog "></i>
                        Carga Masiva
                    </button>*@

            </div>
        </div>
    </div>

    <div cg-busy="{promise:myPromise,message:message,backdrop:true}">
        <div class="widget-box">
            <div class="widget-header">
                <h4 class="smaller" style="text-align:right;">
                    <button ng-hide="inactivaBot" type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important" id="btnNuevo" ng-click="NewArticulo()">
                        <i class="glyphicon glyphicon-plus "></i>
                        Agregar Artículo
                    </button>

                    <button ng-hide="inactivaBot" type="button" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important"
                            id="btnGrabar" ng-click="ConfirmGrabar()">
                        <i class="menu-icon fa fa-save"></i>
                        Grabar Solicitud
                    </button>
                    <button ng-hide="inactivaBot" type="submit" class="btn btn-sm btn-round" style="background-color:rgb(238,37,34)!important;border-color:rgb(238,37,34)!important"
                            id=" btnenviar" ng-click="ConfirmEnviar()">
                        <i class="glyphicon glyphicon-send "></i>
                        Enviar Solicitud
                    </button>
                </h4>
            </div>
        </div>

        <div class="tabbable">
            <ul class="nav nav-tabs" id="myTab">
                <li class="active">
                    <a data-toggle="tab" href="#ListaArticulos">
                        Lista de Artículos
                    </a>
                </li>
                <li>
                    <a data-toggle="tab" href="#DatosGenerales">
                        Datos Generales
                    </a>
                </li>
                <li>
                    <a data-toggle="tab" href="#Medidas">
                        Medidas
                    </a>
                </li>
                <li>
                    <a data-toggle="tab" href="#Imagenes">
                        Imágenes
                    </a>
                </li>
                @*<li>
                        <a data-toggle="tab" ng-hide="false" href="#Compras">
                            Compras
                        </a>
                    </li>*@

            </ul>
            <div class="tab-content">
                <div id="ListaArticulos" class="tab-pane fade in active">
                    <div class="widget-box">
                        <div class="widget-header" style="text-align:center">
                            <h4 class="widget-title">LISTADO ARTÍCULOS</h4>
                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>
                            </div>
                            <font ng-hide="etiTotRegistros==''" class="widget-toolbar"> Total: {{etiTotRegistros}} registros </font>
                            <font ng-hide="etiTotRegistros==''" class="widget-toolbar">
                                <a href="#" ng-click="exportar(1,2)">
                                    <i class="fa fa-file-excel-o " style="text-align:left"></i>
                                    Excel
                                </a>
                            </font>
                            <font ng-hide="etiTotRegistros==''" class="widget-toolbar">
                                <a href="#" ng-click="exportar(2,2)">
                                    <i class="fa fa-file-pdf-o " style="text-align:left"></i>
                                    PDF
                                </a>
                            </font>
                        </div>

                        <div class="widget-body">
                            <div class="widget-main">
                                <div class="row">
                                    <div class=" col-md-12">
                                        <div style=" height: 200px !important;overflow: scroll;">
                                            <div id="no-more-tables">
                                                <table class="table table-striped table-bordered table-condensed table-hover gridStyle">
                                                    <thead>
                                                        <tr>
                                                            <th class="grid-header uppercase info" st-sort="codSAPart" style="font-family:Arial;text-align:center;font-size:11px;" width="10%">
                                                                <a href="#" ng-click="sortType = 'codSAPart'; sortReverse = !sortReverse">
                                                                    Código de Artículo
                                                                    <span ng-show="sortType == 'codSAPart' && !sortReverse" class="fa fa-caret-down"></span>
                                                                    <span ng-show="sortType == 'codSAPart' && sortReverse" class="fa fa-caret-up"></span>
                                                                </a>

                                                            </th>
                                                            <th class="grid-header uppercase info" st-sort="codReferencia" style="font-family:Arial;text-align:center;font-size:11px;" width="12%">
                                                                <a href="#" ng-click="sortType = 'codReferencia'; sortReverse = !sortReverse">
                                                                    Código de Referencia
                                                                    <span ng-show="sortType == 'codReferencia' && !sortReverse" class="fa fa-caret-down"></span>
                                                                    <span ng-show="sortType == 'codReferencia' && sortReverse" class="fa fa-caret-up"></span>
                                                                </a>

                                                            </th>
                                                            <th class="grid-header uppercase info" st-sort="marca" style="font-family:Arial;text-align:center;font-size:11px;" width="10%">
                                                                <a href="#" ng-click="sortType = 'marca'; sortReverse = !sortReverse">
                                                                    Marca
                                                                    <span ng-show="sortType == 'marca' && !sortReverse" class="fa fa-caret-down"></span>
                                                                    <span ng-show="sortType == 'marca' && sortReverse" class="fa fa-caret-up"></span>
                                                                </a>

                                                            </th>
                                                            <th class="grid-header uppercase info" st-sort="descripcion" style="font-family:Arial;text-align:center;font-size:11px;" width="35%">
                                                                <a href="#" ng-click="sortType = 'descripcion'; sortReverse = !sortReverse">
                                                                    Descripción
                                                                    <span ng-show="sortType == 'descripcion' && !sortReverse" class="fa fa-caret-down"></span>
                                                                    <span ng-show="sortType == 'descripcion' && sortReverse" class="fa fa-caret-up"></span>
                                                                </a>

                                                            </th>
                                                            <th class="grid-header uppercase info" st-sort="otroId" style="font-family:Arial;text-align:center;font-size:11px;">
                                                                <a href="#" ng-click="sortType = 'otroId'; sortReverse = !sortReverse">
                                                                    Texto (Otro Idioma)
                                                                    <span ng-show="sortType == 'otroId' && !sortReverse" class="fa fa-caret-down"></span>
                                                                    <span ng-show="sortType == 'otroId' && sortReverse" class="fa fa-caret-up"></span>
                                                                </a>

                                                            </th>
                                                            @*<th class="grid-header uppercase info" st-sort="tipArticulo">
                                                                    <a href="#" ng-click="sortType = 'tipArticulo'; sortReverse = !sortReverse">
                                                                        TIPO DE ARTÍCULO
                                                                        <span ng-show="sortType == 'tipArticulo' && !sortReverse" class="fa fa-caret-down"></span>
                                                                        <span ng-show="sortType == 'tipArticulo' && sortReverse" class="fa fa-caret-up"></span>
                                                                    </a>
                                                                </th>*@
                                                            @*<th class="grid-header uppercase info" st-sort="otroId"><div ng-hide="inactivaBot">Estado</div></th>*@
                                                            <th class="grid-header uppercase info" style="font-family:Arial;text-align:center;font-size:11px;" width="6%">
                                                              <a>
                                                                  Seleccionar
                                                              </a>

                                                            </th>
                                                            <th class="grid-header uppercase info" style="font-family:Arial;text-align:center;font-size:11px;">
                                                                <div ng-hide="isReadOnly">
                                                                    <a>
                                                                        Eliminar
                                                                    </a>
                                                                    
                                                                </div>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="content in grArticulo | orderBy:sortType:sortReverse" ng-hide="content.isGenerico">
                                                            <td style="font-size:11px;font-family:Arial;text-align:center;" data-title="Cód. SAP">{{content.codSAPart}}</td>
                                                            <td style="font-size:11px;font-family:Arial;text-align:left;" data-title="Cód. Referencia">{{content.codReferencia}}</td>
                                                            <td style="font-size:11px;font-family:Arial;text-align:left;" data-title="Marca">{{content.desMarca}}</td>
                                                            <td style="font-size:11px;font-family:Arial;text-align:left;" data-title="Descripción">{{content.descripcion}}</td>
                                                            <td style="font-size:11px;font-family:Arial;text-align:left;" data-title="Texto (Otro Idioma)">{{content.otroId}}</td>
                                                            <td ng-hide="true" style="font-size:11px;font-family:Arial;text-align:center;" data-title="Detalle">{{content.idDetalle}}</td>
                                                            @*<td style="font-size:11px;" data-title="Tipo de Artículo">{{content.tipoArticulo}}</td>*@
                                                            @*<td style="font-size:11px;" data-title="Texto (Otro Idioma)"><div ng-hide="inactivaBot">{{content.estado}}</div></td>*@
                                                            <td style="font-size:11px;font-family:Arial;text-align:center; text-align:center" data-title="Seleccionar"> 
                                                            <a  class="btn btn-danger btn-xs" ng-click="SeleccionArt(content.idDetalle);" 
                                                               style="border-width: 1px;padding: 0 5px;">
                                                                <i class=" glyphicon glyphicon-ok "></i></a> 
                                                            </td>
                                                            <td style="font-size:11px; text-align:center;text-align:center;" data-title="Seleccionar"> <div ng-hide="isReadOnly"><a  class="btn btn-danger btn-xs" ng-click="ConfirmQuitarArticulo(content.codReferencia);" style="border-width: 1px;padding: 0 5px;"><i class=" glyphicon glyphicon-trash "></i></a> </div> </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div id="DatosGenerales" class="tab-pane fade">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="widget-title">Datos Generales</h4>
                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>

                            </div>
                        </div>

                        <div class="widget-body">
                            <div class="widget-main">
                                <form name='requeridodet' ng-submit="NextMedida()">
                                    <br />
                                    <div class="row">
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Código Referencia: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <input ng-disabled="bloqCamposModif" type="text" name="codref" class="form-control" ng-model="p_Det.codReferencia" placeholder="Código Referencia" style=" width: 100%; "
                                                   ng-required="true" oninvalid="setCustomValidity('Ingrese el Código de Referencia.')" oninput="    setCustomValidity('')" />

                                        </div>
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Marca: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-2">
                                            <select id="selectmarca" ng-disabled="inactivaTab || !bloqCamposModif" class="form-control" name="marca" ng-model="ddlMarca" ng-options="item as item.detalle for item in marca"
                                                    style=" width:100%; "
                                                    ng-required="true" oninvalid="setCustomValidity('Seleccione la Marca.')" oninput="    setCustomValidity('')">
                                                <option value="">Seleccione una Marca</option>
                                            </select>

                                        </div>
                                        <div class=" col-md-2">
                                            <input type="text" name="Nombres" class="form-control" id="txtmarcanew" ng-model="p_Det.marcaNueva" ng-disabled="p_Det.viewMarcaNueva" placeholder="Nueva Marca"
                                                   style=" width:100%; " />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-2">
                                            <label>
                                                <span class="lbl">Descripción: *</span>
                                            </label>
                                        </div>
                                        <div class="col-md-4">
                                            <input type="text" ng-disabled="!bloqCamposModif" name="descripcion" class="form-control" id="txtDescripcion" ng-model="p_Det.descripcion" placeholder="Descripción"
                                                   style=" width: 100%;text-transform:uppercase; " data-toggle="tooltip" data-placement="left" title="{{p_Det2.descripcion}}"
                                                   ng-required="true" maxlength="100" oninvalid="setCustomValidity('Ingrese la Descripción.')" oninput="    setCustomValidity('')" ng-disabled="inactivaTab" />

                                        </div>
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Texto Breve:</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <input type="text" ng-disabled="inactivaTab" class="form-control" name="Nombres" id="txtOtroId" ng-model="p_Det.otroId" placeholder="Texto Breve (Otro Idioma)" style=" width: 100%; "
                                                   data-toggle="tooltip" data-placement="left" title="{{p_Det2.otroId}}"  maxlength="200" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">País Origen: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <select id="selectpais" ng-disabled="inactivaTab || !bloqCamposModif" class="form-control" name="paisorigen" ng-model="ddlPaisOrigen" ng-options="item as item.detalle for item in paisOrigen"
                                                    style=" width: 100%; "
                                                    ng-required="true" oninvalid="setCustomValidity('Seleccione País de origen.')" oninput="    setCustomValidity('')">
                                                <option value="">Seleccione un País</option>
                                            </select>
                                        </div>
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Región Origen: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <select id="selectregion" ng-disabled="inactivaTab || !bloqCamposModif" class="form-control" name="regionorigen" ng-model="ddlRegionOrigen" ng-options="item as item.detalle for item in regionOrigen | filter:{descAlterno: ddlPais.codigo}"
                                                    style=" width: 100%; "
                                                    ng-required="true" oninvalid="setCustomValidity('Seleccione Región de origen.')" oninput="    setCustomValidity('')">
                                                <option value="">Seleccione una Región</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Tam./Pres. Artículo: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <input type="text" ng-disabled="inactivaTab || !bloqCamposModif" class="form-control" name="tamanio" id="txtTamArticulo" ng-model="p_Det.tamArticulo" placeholder="Tamaño/Presentación Artículo"
                                                   style=" width: 100%; "
                                                   ng-required="true" maxlength="50" oninvalid="setCustomValidity('Ingrese el tamaño o presentación del artículo.')" oninput="    setCustomValidity('')" />

                                        </div>
                                        <div class=" col-md-2">
                                            <input type="checkbox" ng-disabled="inactivaTab || !bloqCamposModif" name="Nombres" id="chkContAlcohol" ng-model="p_Det.contAlcohol" ng-checked="Det.chkContAlcohol" />
                                            <label>
                                                <span class="lbl">Cont. Alcohol - Grados:</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <select id="selectgrado" ng-disabled="inactivaTab || !bloqCamposModif" class="form-control" name="gradoalcohol" ng-model="ddlGradoAlcohol" ng-options="item as item.detalle for item in gradoAlcohol"
                                                    style=" width:100%; ">
                                                <option value="">Seleccione Grado Alcohol</option>
                                            </select>
                                        </div>
                                    </div>


                                    <div class="row">

                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Modelo: </span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <input type="text" ng-disabled="inactivaTab || !bloqCamposModif" class="form-control" name="modelo" id="txtTamArticulo" ng-model="ddlModelo" placeholder="Modelo"
                                                   style="  width: 100%; " data-toggle="tooltip" data-placement="left" title="{{ddlModelo2}}"
                                                    maxlength="50"  />
                                            @*<select id="selectmodelo" ng-disabled="inactivaTab" class="form-control" name="modelo" ng-model="ddlModelo" ng-options="item as item.detalle for item in modelo"
                                                        style=" width: 100%; "
                                                        ng-required="true" oninvalid="setCustomValidity('Seleccione el modelo de artículo.')" oninput="    setCustomValidity('')">
                                                    <option value="">Seleccione un Modelo</option>
                                                </select>*@
                                        </div>
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Precio Bruto: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <input type="text" ng-disabled="inactivaTab" class="form-control" name="txtPrecioBruto" id="txtPrecioBruto" ng-model="p_Det.precioBruto" placeholder="Precio Bruto"
                                                   onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" style=" width: 100%; "
                                                   ng-required="true" maxlength="13" ng-pattern="/^-?\d{1,8}(\.\d{1,3})?$/" step="0.01" oninvalid="setCustomValidity('Ingrese el Costo sin IVA.')" oninput="    setCustomValidity('')" />

                                        </div>
                                    </div>
                                    <div class="row">

                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">%Descuento 1: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <input type="text" ng-disabled="inactivaTab" class="form-control" name="txtDescuento1" id="txtDescuento1" ng-model="p_Det.descuento1" placeholder="Descuento 1" style=" width: 100%; "
                                                   onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;"
                                                   ng-required="true" maxlength="13" ng-pattern="/^-?\d{1,8}(\.\d{1,3})?$/" step="0.01" oninvalid="setCustomValidity('Ingrese el descuento 1.')" oninput="    setCustomValidity('')" />

                                        </div>
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">%Descuento 2: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <input type="text" ng-disabled="inactivaTab" class="form-control" name="txtDescuento2" id="txtDescuento2" ng-model="p_Det.descuento2" placeholder="Descuento 2" style=" width: 100%; "
                                                   onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;"
                                                   ng-required="true" maxlength="13" ng-pattern="/^-?\d{1,8}(\.\d{1,3})?$/" step="0.01" oninvalid="setCustomValidity('Ingrese el descuento 2.')" oninput="    setCustomValidity('')" />

                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">IVA (Clasificación Fiscal): *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <select id="selectsabor" ng-disabled="inactivaTab || !bloqCamposModif" class="form-control" name="iva" ng-model="ddlIva" ng-options="item as item.detalle for item in clasificacionFiscal"
                                                    style=" width: 100%; "
                                                    ng-required="true" oninvalid="setCustomValidity('Seleccione IVA (Clasificación Fiscal).')" oninput="    setCustomValidity('')">
                                                <option value="">Seleccione la Clasificación Fiscal (IVA)</option>
                                            </select>
                                        </div>
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Deducible: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
                                            <select id="selectmodelo" ng-disabled="inactivaTab || bloqCamposModif" class="form-control" name="modelo" ng-model="ddlDeducible" ng-options="item as item.detalle for item in sn_ded"
                                                    style=" width: 100%; "
                                                    ng-required="true" oninvalid="setCustomValidity('Indique si artículo es deducible')" oninput="    setCustomValidity('')">
                                                <option value="">Seleccione SI/NO</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class=" col-md-2">
                                            <label>
                                                <span class="lbl">Indicador Retención: *</span>
                                            </label>
                                        </div>
                                        <div class=" col-md-4">
