@{
    Layout = "~/Views/Shared/_LayoutAdm.cshtml";
    ViewBag.Title = "Index";
}

@*<div class="breadcrumbs" id="main-container" data-ng-controller="SolicitudProveedorController">*@
@*<div class="ace-settings-container" id="main-container" data-ng-controller="SolicitudProveedorController">*@
      
          <div id="main-container" data-ng-controller="SolicitudProveedorController" style=" font-size:12px;">

              <form name="indexform">
                  @*Session Botones*@
                  <div class="widget-box">
                      <div class="widget-header">
                          <h4 class="smaller" style="text-align:right;">
                              <button type="button" class="btn btn-sm btn-round" style="background-color:darkred!important;border-color:darkred !important" id="btnaccion0" ng-click="SaveProveedorList(' 0')">
                                  <i class="glyphicon glyphicon-edit "></i>
                                  {{Grabar}}
                              </button>
                              <button type="submit" class="btn btn-sm btn-round" style="background-color:darkred!important;border-color:darkred !important" id="btnaccion1" ng-click="SaveProveedorList('1')" data-ng-disabled='!indexform.$valid'>
                                  <i class="glyphicon glyphicon-cog "></i>
                                  {{Cancelar}}
                              </button>
                              <br />
                          </h4>
                      </div>
                  </div>
                  @*Fin Session Botones*@
                  @*Inicio Session Tabs*@

                  <div class="tabbable">
                      <ul class="nav nav-tabs" id="myTab">
                          <li class="active">
                              <a data-toggle="tab" href="#datosproveedor">
                                  Datos del Proveedor
                              </a>
                          </li>
                          <li>
                              <a data-toggle="tab" href="#contactoproveedor">
                                  Contacto
                              </a>
                          </li>
                          <li>
                              <a data-toggle="tab" href="#bancoproveedor">
                                  Banco
                              </a>
                          </li>
                          <li>
                              <a data-toggle="tab" href="#adjuntoproveedor">
                                  Adjunto
                              </a>
                          </li>

                          <li>
                              <a data-toggle="tab" href="#observacionproveedor">
                                  Observaciones
                              </a>
                          </li>
                          <li>
                              <a data-toggle="tab" href="#infocomercialproveedor">
                                  Información Comercial
                              </a>
                          </li>
                      </ul>
                      <div class="tab-content">
                          <!-- #section:Datos del Proveedor -->
                          <div id="datosproveedor" class="tab-pane fade in active">
                              <!--Inicio Identificacion -->

                              <div class="widget-box">
                                  <div class="widget-header">
                                      <h4 class="widget-title">
                                          Identificación

                                      </h4>
                                      <div class="widget-toolbar">
                                          <a href="#" data-action="collapse">
                                              <i class="ace-icon fa fa-chevron-up"></i>
                                          </a>
                                      </div>
                                  </div>
                                  <div class="widget-body">
                                      <div class="widget-main">

                                          <div class="row">
                                              <div class="col-xs-12">
                                                  <div class="form-group">

                                                      <label class="col-sm-2 control-label no-padding-right" for="ddlTipoIndentificacion"> Tipo Identificación </label>

                                                      <div class="col-sm-2">
                                                          <!--<input type="text" id="form-field-1" placeholder="Username" class="col-xs-5 col-sm-5" />-->
                                                          <select name="seletipoidentificacionProveedor" class="col-sm-2 chosen-select form-control" id="ddlTipoIndentificacion" data-ng-model="Ingreso.tipoidentificacion" data-placeholder="Seleccione un Tipo Identificación.." ng-options="item as item.detalle for item in ListTipoIdentificacion"
                                                                  ng-required="true"
                                                                  required data-ng-class='{ error: indexform.seletipoidentificacionProveedor.$invalid && !indexform.$pristine }'>
                                                              <option value="">  </option>

                                                          </select>
                                                          <p3><span data-ng-show="!indexform.$pristine && indexform.seletipoidentificacionProveedor.$error.required" class="red darken-2 center-align white-text">Tipo de Identificacion es requerido.</span></p3>
                                                      </div>

                                                      <label class="col-sm-2 control-label" for="txtIdentificacion"> Identificación </label>

                                                      <div class="col-md-6 ">
                                                          <input type="text" name="txtIdentificacion" placeholder="Identificación" class="col-xs-10 col-sm-5 no-padding" ng-model="p_SolProveedor.Identificacion" maxlength="13" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;"
                                                                 required minlength="10" data-ng-class='{ error: indexform.txtIdentificacion.$invalid && !txtIdentificacion.$pristine }' />
                                                          <p3><span data-ng-show="!indexform.$pristine && indexform.txtIdentificacion.$error.required" class="red darken-2 center-align white-text">La Identificacion es requerido.</span></p3>
                                                          <span ng-if="indexform.txtIdentificacion.$error.minlength && indexform.txtIdentificacion.$touched" class="red darken-2 center-align white-text">La Identificacion es muy pequeña</span>
                                                      </div>
                                                  </div>
                                              </div>

                                              <div class="col-md-12 ">
                                                  <div class="form-group">
                                                      <label class="col-md-2 control-label no-padding-right" for="txtNombreSri"> Nombre SRI </label>

                                                      <div class="col-md-10">

                                                          <input type="text" name="txtNombreSri" ng-model="p_SolProveedor.RazonSocial" maxlength="100" placeholder="Nombre SRI" class="form-control col-md-12 col-md-12"
                                                                 required minlength="4" data-ng-class='{ error: indexform.txtNombreSri.$invalid && !txtNombreSri.$pristine }' />
                                                          <p3><span data-ng-show="!indexform.$pristine && indexform.txtNombreSri.$error.required" class="red darken-2 center-align white-text">Nombre SRI es requerido</span></p3>
                                                          <span ng-if="indexform.txtNombreSri.$error.minlength && indexform.txtNombreSri.$touched" class="red darken-2 center-align white-text">El Nombre SRI es muy pequeño</span>
                                                      </div>


                                                  </div>
                                              </div>
                                              <div class="col-md-12 ">
                                                  <div class="form-group">
                                                      <label class="col-sm-2 control-label no-padding-right" for="txtNombreComercial"> Nombre Comercial </label>
                                                      <div class="col-md-10">
                                                          <input type="text" name="txtNombreComercial" placeholder="Nombre Comercial" ng-model="p_SolProveedor.NomComercial" class="form-control col-md-12 col-md-12" maxlength="35"
                                                                 required minlength="4" data-ng-class='{ error: indexform.txtNombreComercial.$invalid && !txtNombreComercial.$pristine }' />
                                                          <p3><span data-ng-show="!indexform.$pristine && indexform.txtNombreComercial.$error.required" class="red darken-2 center-align white-text">Nombre Comercial es requerido</span></p3>
                                                          <span ng-if="indexform.txtNombreComercial.$error.minlength && indexform.txtNombreComercial.$touched" class="red darken-2 center-align white-text">El Nombre Comercial es muy pequeño</span>
                                                      </div>
                                                  </div>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                              <!--fin Identificacion -->
                              <!--Inicio Ubicacion -->
                              <div class="widget-box">
                                  <div class="widget-header">
                                      <h4 class="widget-title">
                                          Ubicación
                                      </h4>
                                      <div class="widget-toolbar">
                                          <a href="#" data-action="collapse">
                                              <i class="ace-icon fa fa-chevron-up"></i>
                                          </a>
                                      </div>
                                  </div>
                                  <div class="widget-body">
                                      <div class="widget-main">
                                          <div class="row">
                                              <div class="col-md-12">
                                                  <div class="form-group">
                                                      <label class="col-sm-2 control-label no-padding-right" for="ddlPais">Pais </label>

                                                      <div class="col-md-2">
                                                          <!--<input type="text" id="form-field-1" placeholder="Username" class="col-xs-5 col-sm-5" />-->
                                                          <select class="col-md-2 chosen-select form-control" name="selectpaisprov" id="ddlPais" data-placeholder="Seleccione un Pais.." ng-init="Ingreso.Pais=ListPais[0]" data-ng-model=" Ingreso.Pais" ng-options="item as item.detalle for item in ListPais"
                                                                  ng-required="true"
                                                                  required data-ng-class='{ error: indexform.selectpaisprov.$invalid && !indexform.$pristine }'>
                                                              <option value="">  </option>

                                                          </select>
                                                          <p3><span data-ng-show="!indexform.$pristine && indexform.selectpaisprov.$error.required" class="red darken-2 center-align white-text">El Pais es requeirdo</span></p3>
                                                      </div>
                                                      <div class="col-md-2">
                                                          <label class="col-md-2 control-label no-padding-right" for="ddlProvincia">Provincia </label>
                                                      </div>
                                                      <div class="col-md-2">
