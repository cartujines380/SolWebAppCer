                                        <div class="clearfix">
                                            <button type="submit" class="width-35 pull-right btn btn-sm btn-danger" autocomplete="off" value="" ng-class="{'btn-primary':resetPasswordForm.$valid}">
                                                <i class="ace-icon fa fa-lightbulb-o"></i>
                                                <span class="bigger-80">Grabar!</span>
                                            </button>

                                            @*<button type="submit" class="btn btn-default btn-lg btn-block" ng-class="{'btn-primary':forgotPasswordForm.$valid}"
                                                        tabindex="100">
                                                    Reset Password
                                                </button>*@
                                        </div>
                                    </fieldset>
                                </form>
                            </div><!-- /.widget-main -->

                            <div class="toolbar center">
                                <a href="#" data-target="#login-box" class="back-to-login-link" style="color:white !important">
