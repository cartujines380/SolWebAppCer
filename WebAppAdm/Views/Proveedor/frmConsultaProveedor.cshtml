                                                <div ng-hide="true" class="col-sm-2">
                                                    <input type="text" name="txtFax" id="txtFax" ng-disabled="isDisabledApr" placeholder="Fax" class="form-control col-md-12 col-md-12" ng-model="p_SolProveedor.TelfFax" maxlength="18" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" />
                                                </div>



                                                <div ng-hide="true" class="col-md-1">
                                                    <label class="col-md-1 control-label no-padding-right" for="txtFaxExt">Ext: </label>
                                                </div>
                                                <div ng-hide="true" class="col-md-1">
                                                    <input type="text" id="txtFaxExt" ng-disabled="isDisabledApr" placeholder="Ext." class="form-control col-md-9 col-md-9" ng-model="p_SolProveedor.TelfFaxEXT" maxlength="4" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" />
                                                </div>
                                                @*</div>*@
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <div class="col-md-2">
                                                        <label for="TxtCelular"> Teléfono Celular:*</label>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <input type="text" name="TxtCelular" id="TxtCelular" ng-disabled="isDisabledApr" placeholder="Teléfono Celular" class="form-control col-md-12 col-md-12" ng-model="p_SolProveedor.TelfMovil" maxlength="18" onkeypress="if (event.keyCode < 45 || event.keyCode > 57) event.returnValue = false;" ng-pattern="/^\d+$/"
                                                               ng-required="true" oninvalid="setCustomValidity('El Teléfono Celular  requerido')" oninput="    setCustomValidity('')"
                                                               minlength="10" pattern=".{10,}" data-ng-class='{ error: indexform.TxtCelular.$invalid && !TxtCelular.$pristine }' />
                                                    </div>
                                                    <div class="col-md-2">
                                                    </div>
                                                    <div class="col-md-2">
                                                        <label for="txtMailCorpo"> Correo Corporativo:* </label>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="email" name="txtMailCorpo" id="txtMailCorpo" ng-disabled="isDisabledApr" placeholder="Correo Corporativo" class="form-control col-md-12 col-md-12 clsToUpper" ng-model="p_SolProveedor.EMAILCorp" maxlength="40"
                                                               required oninvalid="setCustomValidity('El Correo Coporativo  requerido')" oninput="    setCustomValidity('')"
                                                               minlength="4" data-ng-class='{ error: indexform.txtMailCorpo.$invalid && !txtMailCorpo.$pristine }' />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--fin Contacto de la Empresa -->
                            <!--inicio datos adicionales -->
                            <div class="widget-box">
                                <div class="widget-header">
                                    <h4 class="widget-title" style="font-weight:normal">
                                        Datos Adicionales
                                    </h4>
                                    <div class="widget-toolbar">
                                        <a href="#" data-action="collapse">
                                            <i class="ace-icon fa fa-chevron-up"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="widget-body">
                                    <div class="widget-main">
                                        <div class="row">
                                            <div class="col-md-12">
