@*Session Mensaje Error*@ 
<div class="modal fade" data-backdrop="static" data-keyboard="false" id="idMensajeError" tabindex="-1" role="dialog" aria-hidden="true" style="z-index:98;height:100%; width:100%;">
    <div class="modal-dialog" style="height:80%; top:20%">
        <div class="modal-content">
            <div class="modal-header" style="text-align:center;">
                <div @*class="widget-box"*@>
                    <div class="widget-header" style="color:#ffffff; background:#FF0000 !important;border-color:#FF0000 !important;height:36px">
                        <div class="col-md-1">
                            <button style="background:#FF0000 !important;border-color:#FF0000 !important" class="btn   btn-danger btn-xs">
                                <i class="ace-icon fa fa-close bigger-200"></i>
                            </button>
                        </div>
                        <div class="col-md-9">

                            <h4 style="font-size:14px!important" class="white text-primary bigger-125">
                                Mensaje de Error
                            </h4>
                        </div>
                        <div class="col-md-1">

                        </div>

                    </div>
                </div>
            </div>
            <div class="modal-body" style="  max-height: 70%; overflow-y: auto;">
                <div class="panel-body">
                    <br />
                    <center>
                        <label style="font-size:13px!important">
                            <span class="lbl"><b>{{MenjError}}</b></span>
                        </label>
                    </center>
                    <br />
                </div>
            </div>
            <div class="modal-footer">
                <div style="text-align:center;">
                    <button type="button" class="btn btn-success btn-sm " style="color:#ffffff; background:#FF0000 !important;border-color:#FF0000" data-dismiss="modal" id="btnMsjError">
                        <i class="ace-icon fa fa-check"></i>
                        Aceptar
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
@*Fin Session Mensaje Error*@
